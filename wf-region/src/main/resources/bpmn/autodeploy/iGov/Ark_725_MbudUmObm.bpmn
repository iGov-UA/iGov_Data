    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
        var Sub =' ';
        var Adr =' ';
        var Ind =' ';
        
        if ((execution.getVariable('asOsoba')) === 'sEnamFop') {
	      var ScLN = execution.getVariable('bankIdlastName');
          var ScFN = execution.getVariable('bankIdfirstName');
          var ScMN = execution.getVariable('bankIdmiddleName');
          Sub = ScLN + ' ' + ScFN + ' ' + ScMN;
          
          Adr = execution.getVariable('sAdressRegistration');
          Ind = execution.getVariable('bankIdinn');
        }
        if ((execution.getVariable('asOsoba')) === 'sEnamUrK') {
	      Sub = execution.getVariable('sFullName_SubjectOrgan_01');
	      Adr = execution.getVariable('sLocation_SubjectOrgan_01');
          Ind = execution.getVariable('sID_SubjectOrgan_OKPO_01');
	    }
        execution.setVariable('scrSubject', Sub)
        execution.setVariable('scrAdress', Adr)
        execution.setVariable('scrIdentN', Ind)
      </script>
    </scriptTask>
