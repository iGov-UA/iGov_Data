        </activiti:formProperty>
        <activiti:formProperty id="comment" name="Якщо ви відмовляєте замовнику у наданні послуги, обов'язково вкажіть підставу у цьому полі" type="textArea" default=" "></activiti:formProperty>
        <activiti:formProperty id="sShapka220" name="Шапка для прінтформи; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="markers12" name="принтформа якщо загиблий АТО" type="markers" default="{&quot;motion&quot;:{&quot;ShowElementsOnTrue_12&quot;:{&quot;aElement_ID&quot;:[&quot;ur_pf&quot;],&quot;asID_Field&quot;:{&quot;sCond&quot;:&quot;asChoice&quot;},&quot;sCondition&quot;:&quot;[sCond]=='sEnumATO'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="markers13" name="принтформа якщо фізична особа" type="markers" default="{&quot;motion&quot;:{&quot;ShowElementsOnTrue_13&quot;:{&quot;aElement_ID&quot;:[&quot;fiz_pf&quot;],&quot;asID_Field&quot;:{&quot;sCond&quot;:&quot;asChoice&quot;},&quot;sCondition&quot;:&quot;[sCond]=='sEnumSt'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="sBody1" name="[pattern/print/dneprOblSnap/cnap_473_print1.html]; ;bVisible=false ,bPrintform=true" type="string" default="Форма заяви при перезахороненні загиблого в зоні АТО" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sBody2" name="[pattern/print/dneprOblSnap/cnap_473_print2.html]; ;bVisible=false ,bPrintform=true" type="string" default="Форма заяви при перезахороненні померлого" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_passport" name="Скан-копія паспорту ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile1" name="Копія свідоцтва про смерть, виданого відділом державної реєстрації управління юстиції ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile2" name="Копія лікарського свідоцтва про смерть або довідки про причину смерті, виданих бюро судово-медичної експертизи ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile3" name="Копія протоколу (постанови) слідчих органів про визнання померлої особи ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile4" name="Копія листа бюро судово-медичної експертизи про підтвердження факту смерті та дати поховання загиблого ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
