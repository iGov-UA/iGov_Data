        <activiti:formProperty id="nFile6" name="Копія лікарського свідоцтва про смерть, виданого лікувально-профілактичним закладом за місцем смерті ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile7" name="Копія довідки з лікувально-профілактичного закладу про відсутність у тілі та навколо нього сторонніх предметів ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile8" name="Копія довідки від органів прокуратури чи міліції про можливість вивезення трупу за кордон; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
       
      </extensionElements>
