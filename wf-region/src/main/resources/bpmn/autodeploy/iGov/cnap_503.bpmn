<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="cnap_503" name="Надання згоди на перенесення поховання" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть орган, у якому Ви бажаєте отримати рішення" type="select" default="0" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail для зворотнього зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sAdress" name="Адреса проживання" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameDead" name="ПІБ померлого" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameCemetryOut" name="Назва кладовища, № сектору з якого буде здійснено перепоховання" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameCemetryIn" name="Назва кладовища, № сектору на яке буде здійснено перепоховання" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sReason" name="Причина перепоховання" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="nFile1" name="Висновок органу санітарно-епідеміологічної служби про можливість ексгумації; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nFile2" name="Копія лікарського свідоцтва про смерть; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nFile3" name="Довідка про наявність місць для перепоховання останків на кладовищі, видана місцевим КП; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="назва ЦНАП; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="телефон ЦНАП; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта ЦНАП; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса ЦНАП; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Час роботи ЦНАП; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sShapkaGolova" name="Шапка для прінтформи; ;bVisible=false" type="string"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask2" name="Повідомлення чиновнику" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sMailClerk}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення. Надання згоди на перенесення поховання.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Повідомлення громадянину" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Надання згоди на перенесення поховання.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
			[pattern/mail/new_design/_common_content_start.html]
			<p>
			Ваше звернення про надання згоди на перенесення поховання зареєстровано у системі.<br>
			Повідомлення про результат послуги буде спрямоване на Ваш e-mail.<br><br>
                        ${sPhoneOrgan}
			</p>
			[pattern/mail/new_design/_common_content_end.html]
			[pattern/mail/new_design/_common_signature_start.html]
			${sNameOrgan},<br>
			[pattern/mail/new_design/_common_signature_end.html]
			[pattern/mail/new_design/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] zareestrovano. Detali: igov.org.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <userTask id="usertask1" name="Обробка ЦНАП (1-й крок)" activiti:candidateGroups="cnap_${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Суб’єкт звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-mail для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdress" name="Адреса проживання заявника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sNameDead" name="ПІБ померлого" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sNameCemetryOut" name="Назва кладовища, № сектору з якого буде здійснено перепоховання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sNameCemetryIn" name="Назва кладовища, № сектору на яке буде здійснено перепоховання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sReason" name="Причина перепоховання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sExtensionNumber" name="Реєстраційний номер ЦНАП" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAdminFio" name="П.І.Б. адміністратора ЦНАП, який прийняв заяву" type="string"></activiti:formProperty>
        <activiti:formProperty id="sDateRegistration" name="Вкажіть дату реєстрації (у форматі ДД/ММ/РРРР)" type="string"></activiti:formProperty>
        <activiti:formProperty id="decide" name="Обробка заяви" type="enum" required="true">
          <activiti:value id="accept" name="З документами все гаразд, заява зареєстрована"></activiti:value>
          <activiti:value id="reject" name="Відмовити у розгляді без реєстрації у звязку з відсутністю необхідних документів"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment" name="Якщо ви відмовляєте замовнику у наданні послуги, обов'язково вкажіть підставу у цьому полі" type="textArea" default=" "></activiti:formProperty>
        <activiti:formProperty id="sShapkaGolova" name="Шапка для прінтформи; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sBody" name="[pattern/print/dneprOblSnap/cnap_503_print.html]; ;bVisible=false ,bPrintform=true" type="string" default="Форма заяви гомадянина" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile1" name="Висновок органу санітарно-епідеміологічної служби про можливість ексгумації; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile2" name="Копія лікарського свідоцтва про смерть; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile3" name="Довідка про наявність місць для перепоховання останків на кладовищі, видана місцевим КП; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="servicetask4" name="Повідомлення про відмову" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Надання згоди на перенесення поховання.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
			[pattern/mail/new_design/_common_content_start.html]
			<p>
			На жаль, Ваше звернення щодо отримання рішення міської ради про надання згоди на перенесення поховання було відхилено.<br><br>
			Причина в наступному:<br>
                        ${comment}<br><br>
                        ${sPhoneOrgan}
			</p>
                        <p>За необхідності Ви можете подати ще одне звернення на порталі <a href="https://igov.org.ua/" target="_blank">igov.org.ua</a></p>
			[pattern/mail/new_design/_common_content_end.html]
			[pattern/mail/new_design/_common_signature_start.html]
			${sNameOrgan},<br>
			[pattern/mail/new_design/_common_signature_end.html]
			[pattern/mail/new_design/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] vidhileno. Detali: igov.org.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow7" name="decide=='reject'" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decide=='reject'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask5" name="Повідомлення про реєстрацію" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Надання згоди на перенесення поховання.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
			[pattern/mail/new_design/_common_content_start.html]
			<p>
			Ваше звернення щодо отримання рішення міської ради про надання згоди на перенесення поховання - відправлене в Управління ЖКГ.<br>
			Спеціалісти з органу місцевого самоврядування повинні дати відповідь протягом 30-ти календарних днів з дня реєстрації заяви.<br>
                        ${sPhoneOrgan}
			</p>
			[pattern/mail/new_design/_common_content_end.html]
			[pattern/mail/new_design/_common_signature_start.html]
			${sNameOrgan},<br>
			[pattern/mail/new_design/_common_signature_end.html]
			[pattern/mail/new_design/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] napravleno v upravlinnya. Detali: igov.org.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" name="decide=='accept'" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decide=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask2" name="Обробка ЦНАП (2-й крок)" activiti:candidateGroups="cnap_${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Суб’єкт звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-mail для зворотнього зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdress" name="Адреса проживання заявника" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sNameDead" name="ПІБ померлого" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sNameCemetryOut" name="Назва кладовища, № сектору з якого буде здійснено перепоховання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sNameCemetryIn" name="Назва кладовища, № сектору на яке буде здійснено перепоховання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sReason" name="Причина перепоховання" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sExtensionNumber" name="Реєстраційний номер ЦНАП" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sAdminFio" name="П.І.Б. адміністратора ЦНАП, який прийняв заяву" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sDateRegistration" name="Дата реєстрації ЦНАП" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asStatus" name="Обробка після рішення міської ради" type="enum" required="true">
          <activiti:value id="sEnumGood" name="Рішення готове. Запросити громадянина до ЦНАПу"></activiti:value>
          <activiti:value id="sEnumBad" name="Відправити заявнику копію рішення у разі негативного рішення управління"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="nReference" name="Сканкопія рішення; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nFile1" name="Висновок органу санітарно-епідеміологічної служби про можливість ексгумації; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile2" name="Копія лікарського свідоцтва про смерть; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFile3" name="Довідка про наявність місць для перепоховання останків на кладовищі, видана місцевим КП; ;bNew=true" type="file" writable="false"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow10" sourceRef="servicetask5" targetRef="usertask2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow11" sourceRef="usertask2" targetRef="exclusivegateway2"></sequenceFlow>
    <serviceTask id="servicetask6" name="Повідомлення про готовнисть рішення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Надання згоди на перенесення поховання.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
			[pattern/mail/new_design/_common_content_start.html]
			<p>
			Ваше звернення щодо отримання рішення міської ради про надання згоди на перенесення поховання - опрацьовано.<br>
			Ви або уповноважена Вами особа (при наявності довіреності) можете отримати рішення особисто під підпис у ЦНАП.<br>
                        При собі треба мати:<br>
                        1. Документ, що посвідчує особу.<br>
                        2. Для уповоноваженої особи - довіренність.<br>
                        3. Оригінали документів, що були завантажені при подачі заяви на сайті.<br><br>
                        ${sAddress}<br>
                        ${sWorkTime}<br>
                        ${sPhoneOrgan}
			</p>
			[pattern/mail/new_design/_common_content_end.html]
                        [pattern/mail/new_design/_common_feedback.html]
			[pattern/mail/new_design/_common_signature_start.html]
			${sNameOrgan},<br>
			[pattern/mail/new_design/_common_signature_end.html]
			[pattern/mail/new_design/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA["${nReference}"]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] obrobleno. Detali: igov.org.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask7" name="Повідомлення про негативне рішення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Надання згоди на перенесення поховання.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
			[pattern/mail/new_design/_common_content_start.html]
			<p>
			На жаль, Ваше звернення щодо отримання рішення міської ради про надання згоди на перенесення поховання - відхилено Управлінням ЖКГ.<br>
			Вам надіслано в прикріпленому файлі рішення з вказаною причиною.<br>
