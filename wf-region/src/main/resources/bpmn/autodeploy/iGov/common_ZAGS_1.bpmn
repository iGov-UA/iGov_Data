    <scriptTask id="scripttask3" name="Script Task Transform Date" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var src=execution.getVariable('date_of_visit')

var year=src.substr(0,4)
var month=src.substr(5,2)
var day=src.substr(8,2)
var hour=src.substr(11,2)
var minutes=src.substr(14,2)
var seconds='00'

var timeVisit=day+'.'+month+'.'+year+' '+hour+':'+minutes

execution.setVariable('sWeddingDay', timeVisit)

// assigning to variables for printform

var marriageType = ' '
var medicalInspectione = ' '

if(execution.getVariable('asMarriageType') == 'sEnum1') {
	marriageType = 'Просимо зареєструвати шлюб в неурочистій обстановці.'
}

if(execution.getVariable('asMarriageType') == 'sEnum2') {
	marriageType = 'Просимо зареєструвати шлюб в урочистій обстановці.'
}

if(execution.getVariable('asMedicalInspectione') == 'sEnumYes') {
	medicalInspectione = 'Направлення на медичне обстеження отримали.'
}

if(execution.getVariable('asMedicalInspectione') == 'sEnumNo') {
	medicalInspectione = 'Від отримання направлення на медичне обстеження відмовились.'
}

execution.setVariable('sMarriageType', marriageType)
execution.setVariable('sMedicalInspectione', medicalInspectione)
</script>
