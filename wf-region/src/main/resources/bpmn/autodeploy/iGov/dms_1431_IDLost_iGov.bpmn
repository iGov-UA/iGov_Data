    <serviceTask id="servicetask8" name="Лист заявнику" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
