    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1 = "&lt;li&gt;Документ, що посвідчує особу.&lt;/li&gt;"
var per2 = "&lt;li&gt;Роздруковану заяву, яку ви подавали в електронному вигляді (знаходиться у додатку до листа), засвідчену особистим підписом.&lt;/li&gt;"
var per3 = " "
var per4 = " "
var per5 = " "
var per6 = " "
var per7 = " "
var per8 = " "
var res = 'res'
var res2 = 'res2'
if(execution.getVariable('oplata_1') != null){
per3 = "&lt;li&gt;Платіжний документ (квитанція) про оплату за видачу дубліката.&lt;/li&gt;"
per7 = "&lt;li&gt;Документ про підтвердження внесення реєстраційного збору за видачу дубліката оригіналу установчих документів та змін до них у зв'язку з їх втратою або пошкодженням.&lt;/li&gt;"
}
if(execution.getVariable('oplata_2') != null){
per4 = "&lt;li&gt;Платіжний документ (квитанція) про оплату за публікацію інформації.&lt;/li&gt;"
per8 = "&lt;li&gt;Документ, що підтверджує внесення плати за публікацію у спеціальному друкованому засобі масової інформації повідомлення про втрату оригіналів установчих документів.&lt;/li&gt;"
}
if(execution.getVariable('dovidka') != null){
per5 = "&lt;li&gt;Довідку, видана органом внутрішніх справ, про реєстрацію заяви про втрату оригіналу свідоцтва про реєстрацію та/або статуту.&lt;/li&gt;"
per9 = "&lt;li&gt;Довідка, видана органом внутрішніх справ за місцезнаходженням юридичної особи, про реєстрацію заяви про втрату оригіналів установчих документів.&lt;/li&gt;"
}
if(execution.getVariable('sDocConfidant') != null){
per6 = "&lt;li&gt;Довіреність на право представляти інтереси субʼєкта господарювання в органах місцевого самоврядування.&lt;/li&gt;"
}
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
res2 = '&lt;ol&gt;'+per7+per8+per9+per6+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsLetter', res)
execution.setVariable('sListDocumentsPrint', res2)
	  </script>
