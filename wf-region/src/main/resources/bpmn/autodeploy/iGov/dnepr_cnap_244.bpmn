    <scriptTask id="scripttask2" name="Filelist" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = ' '
var per2 = ' '
var per3 = ' '
var per4 = ' '
var per5 = ' '
var per6 = ' '
var per7 = ' '
var per8 = ' '
var per9 = ' '
var per10 = ' '
var per11 = ' '
var per12 = ' '
var per13 = ' '
var per14 = ' '
var per15 = ' '
var per16 = ' '
var per17 = ' '
var res = 'res'
if (execution.getVariable('vibor_service') == 'issue') {
per1 = '&lt;li&gt;Документ, що підтверджує внесення плати за комплексне вивчення документів та підготовку проекту висновку державної санітарно-епідеміологічної експертизи&lt;/li&gt;'
if(execution.getVariable('nFile_3') != null){
per2 = "&lt;li&gt;Копія висновку державної санітарно-епідеміологічної експертизи на рентгенівський апарат, що встановлений у рентгенівському кабінеті&lt;/li&gt;"
}
if(execution.getVariable('nFile_4') != null){
per3 = "&lt;li&gt;Акт комісії прийняття рентгенівського кабінету в експлуатацію&lt;/li&gt;"
}
if(execution.getVariable('nFile_5') != null){
per4 = "&lt;li&gt;Технічний паспорт рентгенівського кабінету та протоколи періодичного контролю фізико-технічних параметрів рентгенівської апаратури в період експлуатації&lt;/li&gt;"
}
if(execution.getVariable('nFile_6') != null){
per5 = '&lt;li&gt;Акти перевірки ефективності вентиляції&lt;/li&gt;'
}
if(execution.getVariable('nFile_11') != null){
per6 = "&lt;li&gt;Акти випробовувань пристрою захисного заземлення з зазначенням величини опору розтікання струму основних заземлювачів, акти перевірки стану мережі заземлення медичного устаткування й електроустановок, протоколи вимірювань опору ізоляції проводів і кабелів&lt;/li&gt;"
}
if(execution.getVariable('nFile_12') != null){
per7 = "&lt;li&gt;Контрольно-технічний журнал на рентгенівський апарат&lt;/li&gt;"
}
if(execution.getVariable('nFile_13') != null){
per8 = "&lt;li&gt;Протокол дозиметричного контролю засобів захисту в рентгенівському кабінеті, суміжних приміщеннях і на прилеглих територіях&lt;/li&gt;"
}
if(execution.getVariable('nFile_14') != null){
per9 = "&lt;li&gt;Протокол випробування засобів індивідуального захисту та захисних пристроїв апаратів на відповідність свинцевому еквіваленту&lt;/li&gt;"
}
if(execution.getVariable('nFile_15') != null){
per10 = "&lt;li&gt;Інструкції з радіаційної безпеки та запобігання і ліквідації радіаційних аварій&lt;/li&gt;"
}
if(execution.getVariable('nFile_16') != null){
per11 = "&lt;li&gt;Наказ про віднесення осіб, які працюють, до персоналу категорії А&lt;/li&gt;"
}
if(execution.getVariable('nFile_17') != null){
per12 = '&lt;li&gt;Наказ про призначення особи, відповідальної за радіаційну безпеку, радіаційний контроль, та її посадові обов`язки&lt;/li&gt;'
}
if(execution.getVariable('nFile_18') != null){
per13 = "&lt;li&gt;Висновки медичної комісії про проходження персоналом категорії А попереднього (періодичних) медичних оглядів&lt;/li&gt;"
}
if(execution.getVariable('nFile_19') != null){
per14 = "&lt;li&gt;Наказ про допуск персоналу, віднесеного до категорії А, до роботи з джерелами іонізуючого випромінювання&lt;/li&gt;"
}
if(execution.getVariable('nFile_20') != null){
per15 = "&lt;li&gt;Журнал (бази даних) обліку індивідуальних доз опромінення персоналу&lt;/li&gt;"
}
if(execution.getVariable('nFile_21') != null){
per16 = "&lt;li&gt;Журнали реєстрації інструктажу з радіаційної безпеки персоналу категорії А&lt;/li&gt;"
}
if(execution.getVariable('nFile_22') != null){
per17 = "&lt;li&gt;Акт перевірки дотримання санітарного законодавства на діючих підприємствах&lt;/li&gt;"
}
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+per7+per8+per9+per10+per11+per12+per13+per14+per15+per16+per17+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'restructuring') {
per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає переоформленню.&lt;/li&gt;'
per2 = '&lt;li&gt;Документ, який засвідчує настання підстави для переоформлення документу дозвільного характеру.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+per2+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'duplicate') {
per1 = '&lt;li&gt;Бланк документу дозвільного характеру, який непридатний для використання.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'cansel') {
per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає анулюванню.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
}
execution.setVariable('list_documents', res)</script>
