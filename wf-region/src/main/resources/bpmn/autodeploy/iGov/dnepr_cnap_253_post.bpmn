per5 = 'Ваш дозвіл на проведення діагностичних, експериментальних, випробувальних, вимірювальних робіт на підприємствах, в установах та організаціях, діяльність яких пов`язана з використанням джерел неіонізуючого випромінювання - готовий'
per6 = 'видачі'
per7 = 'десяти робочих днів'
}
