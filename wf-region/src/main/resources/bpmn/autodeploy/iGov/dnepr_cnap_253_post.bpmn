per5 = 'Ваш переоформлений дозвіл на проведення діагностичних, експериментальних, випробувальних, вимірювальних робіт на підприємствах, в установах та організаціях, діяльність яких пов`язана з використанням джерел неіонізуючого випромінювання - готовий'
per6 = 'переоформлення'
per7 = 'двох робочих днів'
}
