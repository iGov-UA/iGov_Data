    <scriptTask id="scripttask2" name="List Documents" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = ' '
var per2 = ' '
var per3 = ' '
var per4 = ' '
var per5 = ' '
var per6 = ' '
var res = 'res'
if (execution.getVariable('vibor_service') == 'issue') {
per1 = "&lt;li&gt;Договір оренди або інший документ, що підтверджує право використання відповідного майданчика (об'єкта) для розташування джерел неіонізуючого випромінювання.&lt;/li&gt;"
per2 = "&lt;li&gt;Інформація про технічні характеристики джерел неіонізуючого випромінювання або технічний паспорт зазначених джерел.&lt;/li&gt;"
per3 = "&lt;li&gt;План-схема з позначенням місця розташування джерел неіонізуючого випромінювання.&lt;/li&gt;"
per4 = "&lt;li&gt;Протокол вимірювання рівня неіонізуючого випромінювання, що створюється джерелом неіонізуючого випромінювання, на яке видається дозвіл.&lt;/li&gt;"
per5 = "&lt;li&gt;Висновок державної санітарно-епідеміологічної експертизи щодо ввезення, реалізації та використання сировини, продукції (вироби, обладнання, технологічні лінії тощо) іноземного виробництва в разі відсутності даних щодо їх безпечності для здоров'я населення.&lt;/li&gt;"
per6 = "&lt;li&gt;Розрахунки розподілу рівнів електромагнітного поля на території, що прилягає до джерела неіонізуючого випромінювання, з урахуванням технічних характеристик інших джерел неіонізуючого випромінювання, розташованих у радіусі об'єкта.&lt;/li&gt;"
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'restructuring') {
per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає переоформленню.&lt;/li&gt;'
per2 = '&lt;li&gt;Документ, який засвідчує настання підстави для переоформлення документу дозвільного характеру.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+per2+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'duplicate') {
per1 = '&lt;li&gt;Бланк документу дозвільного характеру, який непридатний для використання.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
}
if (execution.getVariable('vibor_service') == 'cansel') {
per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає анулюванню.&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
}
execution.setVariable('list_documents', res)</script>
