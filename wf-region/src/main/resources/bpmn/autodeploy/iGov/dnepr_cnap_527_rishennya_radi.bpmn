    <scriptTask id="scripttask1" name="Script Task (javascript)" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
    var per1 = 1
    var per6 = ' '
    var perSign = ' '
    
    if (execution.getVariable('nFile_CopyRishennya') != null)
	{ 
		per6 = per6 + per1.toString()+". Копія рішення "+execution.getVariable('sShapka527_rishennyaOrgan')+", до якого необхідно внести зміни. &lt;br&gt;"
		per1 = per1 + 1
	}	
    if (execution.getVariable('nFile_LandManagment') != null)
	{ 
		per6 = per6 + per1.toString()+". Копія документації із землеустрою. &lt;br&gt;"
		per1 = per1 + 1
	}	
    if (execution.getVariable('bankId_scan_passport') != null)
	{ 
		per6 = per6 + per1.toString()+". Копія паспорта. &lt;br&gt;"
		per1 = per1 + 1
	}	
    if (execution.getVariable('bankId_scan_inn') != null)
	{ 
		per6 = per6 + per1.toString()+". Копія ідентифікаційного податкового номеру. &lt;br&gt;"
		per1 = per1 + 1
	}	
    if (execution.getVariable('nFile_AdvertLicense') != null)
	{ 
		per6 = per6 + per1.toString()+". Копія  виписки з єдиного державного реєстру юридичних осіб та фізичних осіб. &lt;br&gt;"
		per1 = per1 + 1
	}	
    if (execution.getVariable('nFile_ConstituentDocuments') != null)
	{ 
		per6 = per6 + per1.toString()+". Копія установчих документів (для суб’єктів господарювання). &lt;br&gt;"
		per1 = per1 + 1
	}	
    if (execution.getVariable('nFile_ConfirmDocum') != null)
	{ 
		per6 = per6 + per1.toString()+". Інші документи, які є підставою для внесення змін до рішень. &lt;br&gt;"
		per1 = per1 + 1
	}	
    execution.setVariable('sAddPrintSpisok', per6)
 
    if (execution.getVariable('sHideSign_iGov') != 'True')
	{ 
		perSign = 'Заявника авторизовано через портал igov.gov.ua, заявка [sID_Order]'
	}	
    execution.setVariable('sPrintSign_iGov', perSign)   
    
      </script>
