    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1 = "&lt;li&gt;Документ, що посвідчує особу&lt;/li&gt;"
var per2 = "&lt;li&gt;Роздруковану заяву, яку ви подавали в електронному вигляді (знаходиться у додатку до листа), засвідчену особистим підписом керівника  юридичної  особи, фізичної особи-підприємця або уповноваженої ними особи&lt;/li&gt;"
var per3 = " "
var per4 = " "
var per5 = " "
var per7 = "res2"
if(execution.getVariable('sDocShow') != null){
per3 = "&lt;li&gt;Копія акта про прийняття в експлуатацію новозбудованого, реконструйованого або переоснащеного кінотеатру, відеозалу, кіноустановки, відеоустановки міжвідомчою комісією.&lt;/li&gt;"
}
if(execution.getVariable('sDocConfidant') != null){
per5 = "&lt;li&gt;Довіреність на право представляти інтереси субʼєкта господарювання в органах місцевого самоврядування.&lt;/li&gt;"
}
if(execution.getVariable('asDirection') == 'production'){
per4 = "Відомості щодо виробництва фільмів - наявність власної або найманої (орендованої) виробничої бази для знімання фільмів: "+execution.getVariable('sProductionBase')+"&lt;br /&gt;Дані про види фільмів, способи їх знімання та виготовлення оригіналів фільмових матеріалів на кіноплівці, магнітних чи інших електронних носіях: "+ execution.getVariable('sProductionType')
}
if(execution.getVariable('asDirection') == 'spread'){
per4 = "Відомості щодо розповсюдження фільмів - наявність власної або найманої (орендованої) виробничої бази для тиражування фільмів на кіноплівці, магнітних чи інших електронних носіях: "+execution.getVariable('sSpreadBase')+"&lt;br /&gt;Види розповсюдження: "+ execution.getVariable('sSpreadType')
}
if(execution.getVariable('asDirection') == 'show'){
per4 = "Відомості щодо демонстрування фільмів - до заяви додаєтся копія акта про прийняття в експлуатацію новозбудованого, реконструйованого або переоснащеного кінотеатру, відеозалу, кіноустановки, відеоустановки міжвідомчою комісією&lt;/li&gt;"
}
res2 = '&lt;ol&gt;'+per1+per2+per3+per5+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsLetter', res2)
execution.setVariable('sListDocumentsPrint', per4)
 </script>
    </scriptTask>
