    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1 = " "
var per2 = " "
var per3 = " "
var per4 = " "
var per5 = " "
var per6 = " "
var per7 = "&lt;li&gt;Роздруковану заяву, яку ви подавали в електронному вигляді (знаходиться у додатку до листа), засвідчену особистим підписом&lt;/li&gt;"
var per8 = "&lt;li&gt;Фотокартки 3*4 (батьків та дітей віком з 6 років) по 1 шт&lt;/li&gt;"
var res = 'res'
var res2 = 'res2'
if(execution.getVariable('sDocMarriage') != null){
per1 = '&lt;li&gt;Копія свідоцтва про шлюб.&lt;/li&gt;'
}
if(execution.getVariable('sDocDovidka') != null){
per2 = '&lt;li&gt;Довідка про те, що за місцем реєстрації батька або матері посвідчення не видавалися.&lt;/li&gt;'
}
if(execution.getVariable('sDocPassportFather') != null){
per3 = "&lt;li&gt;Копія паспорту батька.&lt;/li&gt;"
}
if(execution.getVariable('sDocPassportMother') != null){
per4 = "&lt;li&gt;Копія паспорту матері.&lt;/li&gt;"
}
if(execution.getVariable('sDocPassportChildren') != null){
per5 = "&lt;li&gt;Копії свідоцтв про народження усіх дітей.&lt;/li&gt;"
}
if(execution.getVariable('sDocFamily') != null){
per6 = "&lt;li&gt;Довідка про склад сім’ї.&lt;/li&gt;"
}
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
res2 = '&lt;ol&gt;'+per7+per8+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsPrint', res)
execution.setVariable('sListDocumentsLetter', res2)
	  </script>
    </scriptTask>
