<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="kalush_soczah_401" name="Надання статусу та видача (заміна) посвідчення члена сім’ї загиблого (померлого) ветерана війни" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я " type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти для зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Тип звернення" type="enum" required="true">
          <activiti:value id="sEnumFirst" name="Надання статусу та видача посвідчення «Сім’я загиблого (померлого) ветерана війни»"></activiti:value>
          <activiti:value id="sEnumChange" name="Заміна посвідчення «Сім’я загиблого (померлого) ветерана війни»"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="asCategory" name="Категорія осіб, до якої належав загиблий ветеран" type="enum" required="true">
          <activiti:value id="sEnumArmy" name="Військовослужбовці і працівники ЗСУ, НГ, СБУ, Служби зовнішньої розвідки, Держприкордонслужби, Держспецтрансслужби, військовослужбовців військових прокуратур,МВС, Управління державної охорони, Держспецзв’язку, ДСНС, ДПтС, ін."></activiti:value>
          <activiti:value id="sEnumInvalid" name="Інваліди ВВВ, а також учасники війни, учасники бойових дій визнаних за життя інвалідами"></activiti:value>
          <activiti:value id="sEnumReward" name="Учасники бойових дій та учасники війни, нагороджені за бойові дії державними нагородами та орденами і медалями колишнього СРСР "></activiti:value>
          <activiti:value id="sEnumVolunteer" name="Особи, які добровільно забезпечували проведення АТО (у т.ч здійснювали волонтерську діяльність)"></activiti:value>
          <activiti:value id="sEnumDobrovLaw" name="Особи, які перебували у складі добровольчих формувань, які в подальшому були включені до складу ЗС, МВС, НГ та ін. утворених відповідно до законів України військових формувань та правоохоронних органів"></activiti:value>
          <activiti:value id="sEnumDobrov" name="Особи, які перебували у складі добровольчих формувань, які в подальшому НЕ були включені до складу ЗС, МВС, НГ та ін. утворених відповідно до законів України військових формувань та правоохоронних органів"></activiti:value>
          <activiti:value id="sEnumEmployee" name="Працівники підприємств, установ, організацій, які залучалися до забезпечення проведення АТО"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sPrich" name="Причина заміни посвідчення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="nFamily" name="Документ, що підтверджує родинний зв'язок із загиблим; Свідоцтво про укладення шлюбу або Свідоцтво про народження ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="sLabel1" name="Завантажте документи, необхідні для визначення статусу  особи на яку поширюється дія Закону України “Про статус ветеранів війни, гарантії їх соціального захисту”. Якщо документ має декілька сторінок об'єднайте у pdf-файл або zip архів" type="label" default=" "></activiti:formProperty>
        <activiti:formProperty id="nCertif" name="Посвідчення, що засвідчує статус померлого ветерана війни ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nMSEK" name="Копія довідки МСЕК про причину інвалідності померлого ветерана війни ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nReward" name="Копія посвідчення про нагороди ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nSviDeath" name="Свідоцтво про смерть (копія) або повідомлення про загибель особи ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nVolunteer" name="Договір про здійснення волонтерської діяльності, укладеного з організацією, що залучає волонтерів, або посвідчення волонтера, або витяг з держ.реєстру волонтерів, або копія договору про надання волонтерської допомоги між волонтером і командиром ; ;bNew=true" type="file"></activiti:formProperty>
        <activiti:formProperty id="nCommander" name="Cвідчення командира військової частини (органу, підрозділу), керівника добровольчого формування, які захищали незалежність України та брали безпосередню участь в АТО, про безпосередню участь особи в АТО, завірені печаткою військової частини ; ;bNew=true" type="file"></activiti:formProperty>
        <activiti:formProperty id="nDovidkaVolunteer" name="Довідка керівника АТЦ при СБУ, ГШ ЗСУ про добровільне забезпечення або добровільне залучення до забезпечення проведення АТО особи, яка загинула (пропала безвісти) або померла ; ;bNew=true" type="file"></activiti:formProperty>
        <activiti:formProperty id="nSudMed" name="Висновок судово-медичної експертизи; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nCourt" name="Рішення суду про встановлення факту добровільного забезпечення або добровільного залучення до забезпечення проведення АТО особи, яка загинула (пропала безвісти) або померла ; ;bNew=true" type="file"></activiti:formProperty>
        <activiti:formProperty id="nDovATOvzaemod" name="Довідка керівника АТЦ при СБУ, ГШ ЗСУ про виконання добровольчим формуванням, до складу якого входила особа, завдань АТО у взаємодії із ЗС, МВС, НГ чи ін. утвореними відповідно до законів України військ. формуваннями та правоохоронними органами в ході АТО ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nDokaz" name="Документи про виконання завдань АТО в районах її проведення, а також ін. державні документи, з доказами про участь особи у виконанні завдань АТО, або свідчення двох свідків, які разом з особою брали участь в АТО та мають статус УБД або інваліда війни ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nKlopot" name="Клопотання про надання статусу від керівника добров.формування, документи або письмові свідчення двох свідків з числа осіб, які разом з особою брали участь в АТО та отримали статус УБД/інваліда війни/учасника війни, що підтверджують участь особи в АТО ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nVLK" name="Постанова штатної військово-лікарської комісії відповідного військового формування; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nEmployee" name="Документи про залучення особи до виконання завдань АТО або направлення (прибуття) у відрядження для участі в АТО, а також документи, що були підставою для прийняття керівником рішення про направлення особи у таке відрядження ; ;bNew=true" type="file"></activiti:formProperty>
        <activiti:formProperty id="sSubstance" name="«Сім’я загиблого (померлого) ветерана війни» ; ;bVisible=false" type="string" default="«Сім’я загиблого (померлого) ветерана війни»"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="Телефон для довідок ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="Назва органа ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Час роботи ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта держслужбовця ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sGroup" name="Група користувачів iGov ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="markers1" name="Вибір категорії" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nVLK&quot;,&quot;nDokaz&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumArmy'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_2&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nCertif&quot;,&quot;nMSEK&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumInvalid'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_3&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nCertif&quot;,&quot;nReward&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumReward'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_4&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nVolunteer&quot;,&quot;nCommander&quot;,&quot;nDovidkaVolunteer&quot;,&quot;nSudMed&quot;,&quot;nCourt&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumVolunteer'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_5&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nDovATOvzaemod&quot;,&quot;nDokaz&quot;,&quot;nSudMed&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumDobrovLaw'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_6&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nDovATOvzaemod&quot;,&quot;nKlopot&quot;,&quot;nSudMed&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumDobrov'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_7&quot;:{&quot;aField_ID&quot;:[&quot;sLabel1&quot;,&quot;nSviDeath&quot;,&quot;nFamily&quot;,&quot;nEmployee&quot;,&quot;nSudMed&quot;],&quot;asID_Field&quot;:{&quot;sClient1&quot;:&quot;asCategory&quot;,&quot;sClient2&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient1] == 'sEnumEmployee'&amp;&amp;[sClient2]=='sEnumFirst'&quot;},&quot;ShowFieldsOnCondition_8&quot;:{&quot;aField_ID&quot;:[&quot;sPrich&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'sEnumChange'&quot;}}}"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="servicetask2" name="Повідомлення держслужбовця про звернення" activiti:async="true" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sMailClerk}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення. Надання статусу та видача (заміна) посвідчення «Сім’я загиблого (померлого) ветерана війни»]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Лист громадянину" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] zareestrovano]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему "Отримання (заміна) посвідчення члена сім'ї загиблого ветерана війни" прийняте]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
			<b>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</b><br /><br />
			<p> Ваше звернення №[sID_Order] щодо enum{[asType]} було зареєстровано у системі.<br/>
			Повідомлення про результат послуги буде спрямоване на Ваш e-mail.</p>
			<br />
			${sPhoneOrgan}<br />
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}<br />
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <userTask id="usertask1" name="Розгляд документів держслужбовцем" activiti:candidateGroups="${sGroup}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Суб’єкт звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти для зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Тип звернення" type="enum" writable="false">
          <activiti:value id="sEnumFirst" name="Надання статусу та видача посвідчення «Сім’я загиблого (померлого) ветерана війни»"></activiti:value>
          <activiti:value id="sEnumChange" name="Заміна посвідчення «Сім’я загиблого (померлого) ветерана війни»"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="asCategory" name="Категорія осіб, до якої належав загиблий ветеран" type="enum" writable="false">
          <activiti:value id="sEnumArmy" name="Військовослужбовці і працівники ЗСУ, НГ, СБУ, Служби зовнішньої розвідки, Держприкордонслужби, Держспецтрансслужби, військовослужбовців військових прокуратур,МВС, Управління державної охорони, Держспецзв’язку, ДСНС, ДПтС, ін."></activiti:value>
          <activiti:value id="sEnumInvalid" name="Інваліди ВВВ, а також учасники війни, учасники бойових дій визнаних за життя інвалідами"></activiti:value>
          <activiti:value id="sEnumReward" name="Учасники бойових дій та учасники війни, нагороджені за бойові дії державними нагородами та орденами і медалями колишнього СРСР "></activiti:value>
          <activiti:value id="sEnumVolunteer" name="Особи, які добровільно забезпечували проведення АТО (у т.ч здійснювали волонтерську діяльність)"></activiti:value>
          <activiti:value id="sEnumDobrovLaw" name="Особи, які перебували у складі добровольчих формувань, які в подальшому були включені до складу ЗС, МВС, НГ та ін. утворених відповідно до законів України військових формувань та правоохоронних органів"></activiti:value>
          <activiti:value id="sEnumDobrov" name="Особи, які перебували у складі добровольчих формувань, які в подальшому НЕ були включені до складу ЗС, МВС, НГ та ін. утворених відповідно до законів України військових формувань та правоохоронних органів"></activiti:value>
          <activiti:value id="sEnumEmployee" name="Працівники підприємств, установ, організацій, які залучалися до забезпечення проведення АТО"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sPrich" name="Причина заміни посвідчення" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asEnumStatus" name="Обробка заяви" type="enum" required="true">
          <activiti:value id="sEnumApprove" name="Задовільнити заяву"></activiti:value>
          <activiti:value id="sEnumDeny" name="Відмова громадянину у разі надання неповного пакету документів"></activiti:value>
