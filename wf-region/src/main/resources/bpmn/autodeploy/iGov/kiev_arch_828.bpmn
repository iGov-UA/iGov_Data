    <scriptTask id="scripttask2" name="Отримання дати" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>
def dateArr = new GregorianCalendar()
def dayWeek = dateArr.get(Calendar.DAY_OF_WEEK)
def addDay = 2
if(dayWeek == 5 || dayWeek == 6){ 
	addDay +=2 
} else if(dayWeek == 7) { 
	addDay +=1 
}
dateArr.add(Calendar.DATE, addDay)		
execution.setVariable('dArrival', dateArr.getTime().format("dd.MM.yyyy").toString())
execution.setVariable('outdocDate', new Date().format("dd.MM.yyyy").toString())
      </script>
