        <activiti:formProperty id="sPosition" name="Посада; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sPositionEN" name="Position; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddressCont" name="Адреса; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddressContEN" name="Address; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Телефон; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-мail; ;writable=false" type="string"></activiti:formProperty>
