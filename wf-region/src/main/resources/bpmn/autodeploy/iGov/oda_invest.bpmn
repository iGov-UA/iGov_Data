        <activiti:formProperty id="sCommunalInfrastructureEN" name="Availability and distance to the communal infrastructure;(description of the available or distance to the nearest engineering infrastructure such as electricity, water, gas, sewage). It is important to specify their specifications and comment on the need to increase capacity or upgrade communications for project implementation" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="lResources" name=" ; ;labelType=info" type="label" default="Ресурси та послуги (Resources and services)"></activiti:formProperty>
