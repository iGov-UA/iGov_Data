        <activiti:formProperty id="sPrivileges" name="Наявні або заплановані пільги або преференції (за наявності);Available or planned privileges or preferences" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="sPrivilegesEN" name="Available or planned privileges or preferences;if any" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="lOther" name=" ; ;labelType=info" type="label" default="Інше (Other)"></activiti:formProperty>
        <activiti:formProperty id="sOther" name="Інше (інформація, що може бути корисною для потенційного інвестора (наявність аудійованої фінансової звітності, інші ключові фінансові показники, наявність зацікавлених спів-інвесторів));Other" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="sOtherEN" name="Other;information that may be useful to a potential investor (availability of audited financial statements, other key financial indicators, presence of interested co-investors);if any" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="lContacts" name=" ; ;labelType=info" type="label" default="Контактні дані (Contacts)"></activiti:formProperty>
