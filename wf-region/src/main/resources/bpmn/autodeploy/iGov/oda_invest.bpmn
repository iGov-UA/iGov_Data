        <activiti:formProperty id="sExpectedDuration" name="Очікуваний термін реалізації проекту (програми), кількість років (місяців);Expected duration of the project (program), number of years (months)" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNVP" name="Чиста приведена вартість, (NPV);Net present value (NPV)" type="long" required="true"></activiti:formProperty>
