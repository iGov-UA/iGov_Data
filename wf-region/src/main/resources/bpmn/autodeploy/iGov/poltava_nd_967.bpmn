    <scriptTask id="scripttask1" name="Дата звернення" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>
execution.setVariable('date_today', new Date().format("dd.MM.yyyy").toString())
          
if(execution.getVariable('sID_Public_SubjectOrganJoin') == 'vo_147' &amp;&amp; execution.getVariable('asKind') == 'sAddressing') { 
    execution.setVariable('sMailClerk967', 'natalibukko@gmail.com');
} 
if(execution.getVariable('sID_Public_SubjectOrganJoin') == 'vo_147' &amp;&amp; execution.getVariable('asKind') == 'sAppointment') { 
    execution.setVariable('sMailClerk967', 'valentina.0910@ukr.net');
}
      </script>
