    <scriptTask id="scripttask1" name="Визначення параметрів" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = execution.getVariable('lastName_UA1') + ' '+execution.getVariable('firstName_UA1')+' '+
execution.getVariable('middleName_UA1')+' (адреса реєстрації: '+execution.getVariable('sAdress_1')+
')  \nТелефон: '+execution.getVariable('sphone_1')+
', місце роботи (навчання):  '+execution.getVariable('sPlace_of_Work_1')
var per2 = ''
var per3 = ''

if (execution.getVariable('isRepresentCompany_1') == 'id_Tak')
{
	per1 = per1+' (представник організації: '+execution.getVariable('sNameCompany_1')+
	' (ЄДРПОУ '+execution.getVariable('sedrpou_1')+'))'
}
			
if (execution.getVariable('eCountOrganizatorov') == 'idCount2' || execution.getVariable('eCountOrganizatorov') == 'idCount3')
{
	per2 = per2 + execution.getVariable('lastName_UA2')+' '+execution.getVariable('firstName_UA2')+' '+
				  execution.getVariable('middleName_UA2')+' (адреса реєстрації: '+execution.getVariable('sAdress_2')+
				  ') \nТелефон: '+execution.getVariable('sphone_2')+
				  ', місце роботи (навчання):  '+execution.getVariable('sPlace_of_Work_2')
}

if (execution.getVariable('isRepresentCompany_2') == 'id_Tak' &amp;&amp; ( execution.getVariable('eCountOrganizatorov') == 'idCount2' || execution.getVariable('eCountOrganizatorov') == 'idCount3'))
{
	per2 = per2+' (представник організації: '+execution.getVariable('sNameCompany_2')+
	' (ЄДРПОУ '+execution.getVariable('sedrpou_2')+'))'
}

if (execution.getVariable('eCountOrganizatorov') == 'idCount3')
{
	per3 = per3 + 	execution.getVariable('lastName_UA3')+' '+execution.getVariable('firstName_UA3')+' '+
					execution.getVariable('middleName_UA3')+' (адреса реєстрації: '+execution.getVariable('sAdress_3')+
					') \nТелефон: '+execution.getVariable('sphone_3')+
					', місце роботи (навчання):  '+execution.getVariable('sPlace_of_Work_3')
}
if (execution.getVariable('isRepresentCompany_3') == 'id_Tak' &amp;&amp; execution.getVariable('eCountOrganizatorov') == 'idCount3')
{
	per3 = per3+' (представник організації: '+execution.getVariable('sNameCompany_3')+
	' (ЄДРПОУ '+execution.getVariable('sedrpou_3')+'))'
}

execution.setVariable('sInfoOrg1', per1) 
execution.setVariable('sInfoOrg2', per2)
execution.setVariable('sInfoOrg3', per3)
     </script>
    </scriptTask>
