        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення на тему: Дозвіл на знесення аварійних будівель]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
