    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var sum = 23.11;
if ((execution.getVariable('asLust')) === 'sEnamLustKur') {
	sum =  43.47;
}
execution.setVariable('sSum', sum);  

var postflag = ' ';
if ((execution.getVariable('asVidpovid')) === 'sEnamPost') {
	postflag =  'post';
}
execution.setVariable('FlagPost', postflag); 

        var Sub =' ';
        var Adr =' ';
        var Ind =' ';
        
        if ((execution.getVariable('asOsoba')) === 'sEnamFiz') {
	      var ScLN = execution.getVariable('bankIdlastName');
          var ScFN = execution.getVariable('bankIdfirstName');
          var ScMN = execution.getVariable('bankIdmiddleName');
          Sub = ScLN + ' ' + ScFN + ' ' + ScMN;
          
          Adr = execution.getVariable('bankIdAddressFactual');
          Ind = execution.getVariable('bankIdinn');
        }
        if ((execution.getVariable('asOsoba')) === 'sEnamUrK'||(execution.getVariable('asOsoba')) === 'sEnamUrP') {
	      Sub = execution.getVariable('sFullName_SubjectOrgan_01');
	      Adr = execution.getVariable('sLocation_SubjectOrgan_01');
          Ind = execution.getVariable('sID_SubjectOrgan_OKPO_01');
	    }
        execution.setVariable('scrSubject', Sub)
        execution.setVariable('scrAdress', Adr)
        execution.setVariable('scrIdentN', Ind)

    
var num1 = [ "otr", "orend", "podatok"]
var num2 = [ "Для отримання земельної ділянки у власність", "Для оформлення договору оренди", "Для подачі в податкову для нарахування податку на землю"]
for (var i = 0; i&lt;3; i++){
var per5 = num1[i]
if(execution.getVariable('goal') == per5){
var per6 = num2[i]
break}}
execution.setVariable('CLgoal', per6)

var per10 = 'no_dgk'
if(execution.getVariable('sID_Public_SubjectOrganJoin') == 'magdal')
{var per10 = 'yes_dgk'}

execution.setVariable('sflag_dgk', per10)
      </script>
    </scriptTask>
