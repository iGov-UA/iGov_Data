    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
        var sum ='0.53'
      	var des ='державне мито за повторну видача свідоцтва про народження'
        execution.setVariable('sSum', sum)
        execution.setVariable('sDescription', des)
        
        var Z_LastName=''
        var Z_FirstName=''
        var Z_MiddleName=''
        var M_Name=''
        var D_Name=''
        var С_Name=''
        var M_PIB=''
        var D_PIB=''
        var С_PIB=''
        
        Z_LastName=execution.getVariable('bankIdlastName')
        Z_FirstName=execution.getVariable('bankIdfirstName')
        Z_MiddleName=execution.getVariable('bankIdmiddleName')
        M_Name=execution.getVariable('sMom_Name')
        D_Name=execution.getVariable('sDad_Name')
        С_Name=execution.getVariable('sZap_Name')
        
        if(execution.getVariable('asOtrymuvach') == 'sEnamSobi')
          {
          С_PIB=Z_LastName+' '+Z_FirstName+' '+Z_MiddleName
          D_PIB=D_Name
          M_PIB=M_Name
          }
        if(execution.getVariable('asOtrymuvach') == 'sEnamInshOs')
          {
          С_PIB=С_Name
          }
        
        if(execution.getVariable('asRodich') == 'sEnamTato')
          {
          D_PIB=Z_LastName+' '+Z_FirstName+' '+Z_MiddleName
          M_PIB=M_Name
          }
        if(execution.getVariable('asRodich') == 'sEnamMatu')
          {
          M_PIB=Z_LastName+' '+Z_FirstName+' '+Z_MiddleName
          D_PIB=D_Name
          }
        if(execution.getVariable('asRodich') == 'sEnamOpikun')
          {
          D_PIB=D_Name
          M_PIB=M_Name
          }
        if(execution.getVariable('asRodich') == 'sEnamUsin')
          {
          D_PIB=D_Name
          M_PIB=M_Name
          }
        if(execution.getVariable('asRodich') == 'sEnamZdor')
          {
          D_PIB=D_Name
          M_PIB=M_Name
          }
        if(execution.getVariable('asRodich') == 'sEnamOpikOrg')
          {
          D_PIB=D_Name
          M_PIB=M_Name
          }
        if(execution.getVariable('asRodich') == 'sEnamPredst')
          {
          D_PIB=D_Name
          M_PIB=M_Name
          }      
        execution.setVariable('sPIBMom', M_PIB)
        execution.setVariable('sPIBDad', D_PIB)
        execution.setVariable('ChildFio', С_PIB)
      </script>
    </scriptTask>
