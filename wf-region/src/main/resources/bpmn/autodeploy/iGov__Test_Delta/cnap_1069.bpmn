<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="cnap_1069" name="Документи про уповноваження з розслідування нещасного випадку" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я " type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sSocBenefit" name="Соціальний стан (пільги, група інвалідності)" type="string"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти для зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Тип нещасного випадку" type="enum" required="true">
          <activiti:value id="sEnumWork" name="виробничий"></activiti:value>
          <activiti:value id="sEnumHome" name="невиробничий"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sDate" name="Дата, коли стався нещасний випадок" type="date" required="true"></activiti:formProperty>
        <activiti:formProperty id="sTime" name="Час, коли стався нещасний випадок; вкажіть в форматі гг:хх" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sAccidentAddress" name="Адреса, де саме стався нещасний випадок" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sAccident" name="Опишіть обставини, що спричинили нещасний випадок; не більше 4000 символів" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="nSick" name="Листок непрацездатності з лікувально-профілактичного закладу ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nReport" name="Повідомлення про нещасний випадок з лікувально-профілактичного закладу ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nHospital" name="Довідка з лікувально-профілактичного закладу ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="Телефон для довідок; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="Назва органа; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Час роботи; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта держслужбовця; ;bVisible=false" type="string"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="servicetask2" name="Повідомлення держслужбовця про звернення" activiti:async="true" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sMailClerk}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення. Видача документів про уповноваження з розслідування нещасного випадку]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Направлення громадянину листа про отримання звернення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] zareestrovano]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему "Видача документів про уповноваження з розслідування нещасного випадку" прийняте]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
<b>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</b><br /><br />
<p> Ваше звернення №[sID_Order] на тему: "Видача документів про уповноваження з розслідування нещасного випадку" було зареєстровано у системі.<br/>
Повідомлення про результат послуги буде спрямоване на Ваш e-mail.</p>
<br />
${sPhoneOrgan}<br />
[pattern/mail/_common_signature_start.html]
${sNameOrgan}<br />
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <userTask id="usertask1" name="Розгляд документів держслужбовцем" activiti:candidateGroups="cnap_${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="sNameOrgan" name="Назва органа; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="clFio" name="Суб’єкт звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sSocBenefit" name="Соціальний стан (пільги, група інвалідності); ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти для зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Тип нещасного випадку; ;writable=false" type="enum">
          <activiti:value id="sEnumWork" name="виробничого"></activiti:value>
          <activiti:value id="sEnumHome" name="невиробничого"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sDate" name="Дата, коли стався нещасний випадок; ;writable=false" type="date"></activiti:formProperty>
        <activiti:formProperty id="sTime" name="Час, коли стався нещасний випадок; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAccidentAddress" name="Адреса, де саме стався нещасний випадок; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAccident" name="Обставини, що спричинили нещасний випадок; ;writable=false" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="decide" name="Обробка заяви" type="enum" required="true">
          <activiti:value id="accept" name="Задовільнити заяву"></activiti:value>
          <activiti:value id="reject" name="Відмова громадянину у разі надання неповного пакету документів"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment" name="Якщо ви відмовляєте замовнику у наданні послуги, обов'язково вкажіть підставу у цьому полі." type="textArea"></activiti:formProperty>
        <activiti:formProperty id="PrintForm_1" name="File label;File title;pattern/print/dneprOblSnap/cnap_1069_print.html" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sBody1" name="[pattern/print/dneprOblSnap/cnap_1069_print.html]; ;bVisible=false ,bPrintform=true" type="string" default="Заява громадянина" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nSick" name="Листок непрацездатності з лікувально-профілактичного закладу ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nReport" name="Повідомлення про нещасний випадок з лікувально-профілактичного закладу ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nHospital" name="Довідка з лікувально-профілактичного закладу ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
       
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
       
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow5" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <serviceTask id="servicetask4" name="Лист про неповний пакет документів" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] obrobleno. Detali: igov.gov.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Інформація щодо Видачі документів про уповноваження з розслідування нещасного випадку]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
			<b>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</b><br /><br />
			<p> На жаль, Ваше звернення на тему: "Видача документів про уповноваження з розслідування нещасного випадку" було відхилено.<br/>
			<p> Причина в наступному:<br/>
			<p>${comment}</p>
			<p>Звертаємо увагу, Ви можете заповнити заявку повторно на порталі <a href="https://igov.gov.ua/ target="_blank">igov.gov.ua</a></p>
			<br />
			${sPhoneOrgan}<br />
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}<br />
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow7" name="Недостатньо документів" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decide == 'reject'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask5" name="Лист з запрошенням для отримання посвідчення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] obrobleno. Detali: igov.gov.ua/journal abo u Vashomu email]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Запрошення до отримання документів про уповноваження з розслідування нещасного випадку]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
			<b>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</b><br /><br />
			<p>Ваше звернення на тему: "Видача документів про уповноваження з розслідування нещасного випадку" - розглянуте.</p>
			<p>Коментар держслужбовця (за наявності): ${comment}</p>
			<p>Запрошуємо Вас отримати документи за адресою:<br/>
			</p>
			${sPhoneOrgan}<br />
			<br />
			<br />
			${sWorkTime}<br />
			${sAddress}<br />
			<br />
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}<br />
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_feedback.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow9" name="Повний пакет документів" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decide == 'accept'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="servicetask5" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="startevent1" targetRef="servicetask2"></sequenceFlow>
  </process>
