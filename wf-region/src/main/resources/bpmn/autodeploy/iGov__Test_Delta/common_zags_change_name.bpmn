    <scriptTask id="scripttask1" name="Формування переліку документів" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1 = "&lt;li&gt;Документ, що посвідчує особу (паспорт, свідоцтво про народження);&lt;/li&gt;"
var per2 = "&lt;li&gt;Заява, яка була подана в електронному вигляді (знаходиться у додатку до листа). Завантажте, роздрукуйте і підпишіть її, будь ласка;&lt;/li&gt;"
var per4 = "&lt;li&gt;Свідоцтво про народження;&lt;/li&gt;"
var per5 = "&lt;li&gt;Кольорова фотокартка для документів - розмір 3х4;&lt;/li&gt;"
var per6 = " "
var per7 = " "
var per8 = " "
var per9 = " "
var per10 = " "
var per11 = " "
var per12 = " "
var per13 = "&lt;li&gt;Фотокартка заявника;&lt;/li&gt;"
var per14 = " "
var per15 = " "
var per16 = " "
var per17 = "&lt;li&gt;Квитанція про сплату державного мита;&lt;/li&gt;"
var res = 'res'
var res2 = 'res2'
if(execution.getVariable('sDocPay') != null){
per6 = '&lt;li&gt;Квитанція про сплату державного мита;&lt;/li&gt;'
}
if(execution.getVariable('sDocMarriage') != null){
per7 = '&lt;li&gt;Свідоцтво про шлюб;&lt;/li&gt;'
}
if(execution.getVariable('sDocNoMarriage') != null){
per8 = '&lt;li&gt;Документ про розірвання шлюбу (або інший документ, який підтверджує припинення шлюбу);&lt;/li&gt;'
}
if(execution.getVariable('saChildren') == 'yes'){
per9 = "&lt;li&gt;Свідоцтва про народження дітей;&lt;/li&gt;"
}
if(execution.getVariable('sDocPrevious') != null){
per10 = "&lt;li&gt;Документ, що підтверджує зміну імені;&lt;/li&gt;"
}
if(execution.getVariable('sDocDeath') != null){
per11 = "&lt;li&gt;Свідоцтво про смерть другого із подружжя;&lt;/li&gt;"
}
if(execution.getVariable('sDocPassport') != null){
per12 = "&lt;li&gt;Копія документа, що посвідчує особу;&lt;/li&gt;"
}
if(execution.getVariable('asFullName') == 'name'){
per14 = "прізвище, &lt;ins&gt;власне ім’я&lt;/ins&gt;, по батькові "
}
if(execution.getVariable('asFullName') == 'surname'){
per14 = "&lt;ins&gt;прізвище&lt;/ins&gt;, власне ім’я, по батькові"
}
if(execution.getVariable('asFullName') == 'secondname'){
per14 = "прізвище, власне ім’я,&lt;ins&gt; по батькові&lt;/ins&gt;"
}
if(execution.getVariable('asFullName') == 'namesurname'){
per14 = "&lt;ins&gt;прізвище, власне ім’я&lt;/ins&gt;, по батькові"
}
if(execution.getVariable('asFullName') == 'pib'){
per14 = "&lt;ins&gt;прізвище, власне ім’я, по батькові&lt;/ins&gt;"
}

if(execution.getVariable('asFullName') == 'name'){
per15 = execution.getVariable('sName')
}
if(execution.getVariable('asFullName') == 'surname'){
per15 = execution.getVariable('sSurname')
}
if(execution.getVariable('asFullName') == 'secondname'){
per15 = execution.getVariable('sSecondName')
}
if(execution.getVariable('asFullName') == 'namesurname'){
per15 = execution.getVariable('sSurname')+" "+execution.getVariable('sName')
}
if(execution.getVariable('asFullName') == 'pib'){
per15 = execution.getVariable('sSurname')+" "+execution.getVariable('sName')+" "+execution.getVariable('sSecondName')
}

res = '&lt;ol&gt;'+per4+per7+per8+per9+per10+per11+per12+per13+per17+'&lt;/ol&gt;'
res2 = '&lt;ol&gt;'+per1+per2+per4+per5+per6+per7+per8+per9+per10+per11+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsPrint', res)
execution.setVariable('sListDocumentsLetter', res2)
execution.setVariable('asFullNamepf', per14)
execution.setVariable('sNowName', per15)

var unixdate= Math.round((new Date()).getTime());
var new_src = new Date(unixdate);
var d = new_src.getDate();
if (d &lt; 10) d = "0"+d;
var m = new_src.getMonth();
var m = (+m + +1);
if (m &lt; 10) m = "0"+m;
var new_year = new_src.getFullYear();
var PrintformDate = d+'/'+m+'/'+new_year;
execution.setVariable('PrintformDate', PrintformDate)

	  </script>
