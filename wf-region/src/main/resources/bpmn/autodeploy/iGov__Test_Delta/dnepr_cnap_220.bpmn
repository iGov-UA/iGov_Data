    <scriptTask id="scripttask2" name="Script list_documents" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
		var per1 = ' '
		var per2 = ' '
		var per3 = ' '
		var per4 = ' '
		var per5 = ' '
		var per6 = ' '
		var per7 = ' '
		var per8 = ' '
		var per9 = ' '
		var per10 = ' '
		var res = 'res'
		if (execution.getVariable('asVibor_service') == 'Видача висновку') 
		{
		per1 = '&lt;li&gt;Документ, що підтверджує внесення плати за комплексне вивчення документів та підготовку проекту висновку державної санітарно-епідеміологічної експертизи.&lt;/li&gt;'
		per2 = "&lt;li&gt;Спеціфікація (довідка про повний склад об'єкта експертизи та показники його ідентифікації).&lt;/li&gt;"
		per3 = "&lt;li&gt;Документ власника, що декларує відповідність об'єкта експертизи визначеним в Україні вимогам щодо їх безпеки для здоров'я людини.&lt;/li&gt;"
		per4 = "&lt;li&gt;Протоколи досліджень об'єкта експертизи за показниками якості та безпеки.&lt;/li&gt;"
		if(execution.getVariable('nFile_6') != null)
		{
		per5 = '&lt;li&gt;Акт про обстеження підприємства.&lt;/li&gt;'
		}
		if(execution.getVariable('nFile_11') != null)
		{
		per6 = '&lt;li&gt;Нормативний документ, та технічна документація.&lt;/li&gt;'
		}
		if(execution.getVariable('nFile_12') != null)
		{
		per7 = '&lt;li&gt;Копія документа, що підтверджує право власності на приміщення або право користування ним.&lt;/li&gt;'
		}
		if(execution.getVariable('nFile_13') != null)
		{
		per8 = '&lt;li&gt;Копія документа, який підтверджує прийняття в експлуатацію закінченого будівництвом об’єкта&lt;/li&gt;'
		}
		if(execution.getVariable('nFile_14') != null)
		{
		per9 = '&lt;li&gt;Асортиментний перелік продукції.&lt;/li&gt;'
		}
		if(execution.getVariable('nFile_15') != null)
		{
		per10 = '&lt;li&gt;Документи щодо земельної ділянки: відведення / права власності / користування.&lt;/li&gt;'
		}
		res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+per7+per8+per9+per10+'&lt;/ol&gt;'
		}
		if (execution.getVariable('asVibor_service') == 'Переоформлення висновку') 
		{
		per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає переоформленню.&lt;/li&gt;'
		per2 = '&lt;li&gt;Документ, який засвідчує настання підстави для переоформлення документу дозвільного характеру.&lt;/li&gt;'
		res = '&lt;ol&gt;'+per1+per2+'&lt;/ol&gt;'
		}
		if (execution.getVariable('asVibor_service') == 'Видача дублікату висновку') 
		{
		per1 = '&lt;li&gt;Бланк документу дозвільного характеру, який непридатний для використання.&lt;/li&gt;'
		res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
		}
		if (execution.getVariable('asVibor_service') == 'Анулювання висновку') 
		{
		per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає анулюванню.&lt;/li&gt;'
		res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
		}
		execution.setVariable('list_documents', res)</script>
    </scriptTask>
