    <scriptTask id="scripttask1" name="Script pulling data" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
		var per4 = 'substance'
		var per5 = 'substance_2'
		var per6 = 'substance_3'
		var per7 ='term'
		if (execution.getVariable('asVibor_service') == 'Видача висновку')
		{
		per4 = 'видати'
		per5 = 'Ваш висновок державної санітарно-епідеміологічної експертизи діючих об’єктів - готовий'
		per6 = 'видачі'
		per7 = 'тридцяти календарних днів'
		}
		if (execution.getVariable('asVibor_service') == 'Переоформлення висновку')
		{
		per4 = 'переоформити'
		per5 = 'Ваш переоформлений висновок державної санітарно-епідеміологічної експертизи діючих об’єктів - готовий'
		per6 = 'переоформлення'
		per7 = 'двох робочих днів'
		}
		if (execution.getVariable('asVibor_service') == 'Видача дублікату висновку')
		{
		per4 = 'видати дублікат'
		per5 = 'Ваш дублікат висновку державної санітарно-епідеміологічної експертизи діючих об’єктів - готовий'
		per6 = 'видачі дублікату'
		per7 = 'двох робочих днів'
		}
		if (execution.getVariable('asVibor_service') == 'Анулювання висновку')
		{
		per4 = 'анулювати'
		per5 = 'Рішення про анулювання документа дозвільного характеру - готове'
		per6 = 'анулювання'
		per7 = 'п`яти робочих днів'
		}
		execution.setVariable('substance', per4)
		execution.setVariable('substance_2', per5)
		execution.setVariable('substance_3', per6)
		execution.setVariable('term', per7)
	  </script>
    </scriptTask>
