        <activiti:formProperty id="nYearOfManufacture" name="Рік випуску" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="sCountryManufacture" name="Виробник (держава)" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sIdentificationNumber" name="Ідентифікаційні номери (заводський, шасі/рами)" type="string" required="true"></activiti:formProperty>
