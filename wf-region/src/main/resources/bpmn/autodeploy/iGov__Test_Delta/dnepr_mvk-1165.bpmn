    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var typeMail = execution.getVariable('asLust');
var sum = 15.78;
if (typeMail === 'sEnamLustKur') {
	sum =  36.14;
}
execution.setVariable('sSum', sum);

var adres = '';
var workTime = '';
switch (asDistrict){
case 'sSobor':
adres = "пр. Героїв, 20";
workTime = "понеділок, вівторок, четвер з 8:00 до 17:00&lt;br&gt; п'ятниця з 8:00 до 15:45";
break;
case 'sChech':
adres = "пр. Б.Хмельницького (вул. Героїв Сталінграда), 14";
workTime = "понеділок, вівторок, четвер з 8:00 до 17:00 &lt;br&gt; п'ятниця з 8:00 до 15:45";
break;
case 'sShev':
adres = "пр. Б.Хмельницького (вул. Героїв Сталінграда), 14";
workTime = "понеділок, вівторок, четвер з 8:00 до 17:00 &lt;br&gt; п'ятниця з 8:00 до 15:45";
break;
default:
    alert('ДЛЯ ВАШОГО БУДИНКУ ПОСЛУГУ НАДАТИ НЕ МОЖЛИВО!');
    break;
}
execution.setVariable('sAddress', adres);
execution.setVariable('sWorkDay', workTime);
</script>
    </scriptTask>
