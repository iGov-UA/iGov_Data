    <scriptTask id="scripttask1" name="Script ListDocuments" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = ' '
var per2 = ' '
var per3 = ' '
var per4 = ' '
var per5 = ' '
var per6 = ' '
var per7 = ' '
var res = 'res'
if (execution.getVariable('sChoiceObject') == 'fiz' &amp;&amp; execution.getVariable('sChoiceATO') == 'yes') {
per1 = '&lt;li&gt;копія паспорта громадянина України;&lt;/li&gt;'
per2 = "&lt;li&gt;копія довідки про присвоєння ідентифікаційного номера;&lt;/li&gt;"
per3 = "&lt;li&gt;довідка учасника антитерористичної операції;&lt;/li&gt;"
per4 = "&lt;li&gt;письмова згода землекористувача, засвідчена нотаріально (у разі вилучення земельної ділянки) та копії паспорта громадянина України та довідки про присвоєння ідентифікаційного номера або копії установчих документів юридичної особи;&lt;/li&gt;"
per5 = '&lt;li&gt;графічні матеріали, що забезпечують чітке відображення всіх елементів та написів, що містить графічне зображення земельної ділянки із зазначенням місця розташування її меж;&lt;/li&gt;'
per6 = '&lt;li&gt;довіреність, на підставі якої інтереси заявника представляє його уповноважена особа, або її копія, засвідчена у встановленому порядку (якщо клопотання подається представником).&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
}
if (execution.getVariable('sChoiceObject') == 'fiz' &amp;&amp; execution.getVariable('sChoiceATO') == 'no') {
per1 = '&lt;li&gt;копія паспорта громадянина України;&lt;/li&gt;'
per2 = "&lt;li&gt;копія довідки про присвоєння ідентифікаційного номера;&lt;/li&gt;"
per3 = "&lt;li&gt;письмова згода землекористувача, засвідчена нотаріально (у разі вилучення земельної ділянки) та копії паспорта громадянина України та довідки про присвоєння ідентифікаційного номера або копії установчих документів юридичної особи;&lt;/li&gt;"
per4 = "&lt;li&gt;правовстановлюючих документів на нерухоме майно;&lt;/li&gt;"
per5 = "&lt;li&gt; технічного паспорта на об'єкт нерухомого майна;&lt;/li&gt;"
per6 = '&lt;li&gt;графічні матеріали, що забезпечують чітке відображення всіх елементів та написів, що містить графічне зображення земельної ділянки із зазначенням місця розташування її меж;&lt;/li&gt;'
per7 = '&lt;li&gt;довіреність, на підставі якої інтереси заявника представляє його уповноважена особа, або її копія, засвідчена у встановленому порядку (якщо клопотання подається представником).&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+per7+'&lt;/ol&gt;'
}
if (execution.getVariable('sChoiceObject') == 'ur') {
per1 = '&lt;li&gt;копії установчих документів та свідоцтва про державну реєстрацію юридичної особи або виписки/витягу з Єдиного державного реєстру юридичних осіб та фізичних осіб – підприємців;&lt;/li&gt;'
per2 = "&lt;li&gt;письмова згода землекористувача, засвідчена нотаріально (у разі вилучення земельної ділянки) та копії паспорта громадянина України та довідки про присвоєння ідентифікаційного номера або копії установчих документів юридичної особи;&lt;/li&gt;"
per3 = "&lt;li&gt;правовстановлюючих документів на нерухоме майно;&lt;/li&gt;"
per4 = "&lt;li&gt; технічного паспорта на об'єкт нерухомого майна;&lt;/li&gt;"
per5 = '&lt;li&gt;графічні матеріали, що забезпечують чітке відображення всіх елементів та написів, що містить графічне зображення земельної ділянки із зазначенням місця розташування її меж;&lt;/li&gt;'
per6 = '&lt;li&gt;довіреність, на підставі якої інтереси заявника представляє його уповноважена особа, або її копія, засвідчена у встановленому порядку (якщо клопотання подається представником).&lt;/li&gt;'
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+'&lt;/ol&gt;'
}
execution.setVariable('sList_documents', res)</script>
