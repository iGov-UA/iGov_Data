<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="dvs_622" name="Отримання відомостей про платіжні реквізити Державної виконавчої служби" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть потрібний відділ ДВС" type="select" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Вкажіть електронну адресу, на яку отримаєте платіжні реквізити" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="Назва ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="Телефон ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Графік роботи ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sRekv" name="Платіжні реквізити ; ;bVisible=false" type="string"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask2" name="Лист з реквізитами" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Платіжні реквізити державної виконавчої служби]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
			<b>Добрий день!</b><br /><br />
			<p> На Ваш запит щодо отримання платіжних реквізитів Державної виконавчої служби повідомляємо наступне:<br/><br/>
			${sNameOrgan}<br/><br/>
			${sRekv}</p>
			<br />
			${sPhoneOrgan}<br />
			${sAddress}<br />
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}<br />
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow4" sourceRef="servicetask2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="startevent1" targetRef="servicetask2"></sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_dvs_622">
    <bpmndi:BPMNPlane bpmnElement="dvs_622" id="BPMNPlane_dvs_622">
      <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="130.0" y="120.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="servicetask2" id="BPMNShape_servicetask2">
        <omgdc:Bounds height="55.0" width="105.0" x="385.0" y="110.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="296.0" y="190.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="437.0" y="165.0"></omgdi:waypoint>
        <omgdi:waypoint x="313.0" y="190.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="165.0" y="137.0"></omgdi:waypoint>
        <omgdi:waypoint x="385.0" y="137.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>