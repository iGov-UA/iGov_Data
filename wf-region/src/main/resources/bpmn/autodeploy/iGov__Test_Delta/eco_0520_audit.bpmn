    <serviceTask id="servicetask9" name="повідомлення заявника про дату та час іспиту" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
