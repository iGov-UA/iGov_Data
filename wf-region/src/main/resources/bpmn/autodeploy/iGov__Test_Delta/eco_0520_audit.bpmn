    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = "&lt;li&gt;Документ, що посвідчує особу.&lt;/li&gt;"
var per2 = "&lt;li&gt;Роздрукована заява, яку ви подавали в електронному вигляді (знаходиться у додатку до листа), засвідчена особистим підписом.&lt;/li&gt;"
var per3 = "&lt;li&gt;Особиста картка екологічного аудитора (знаходиться у додатку до листа), засвідчена особистим підписом.&lt;/li&gt;"
var per4 = " "
var per5 = " "
var per6 = " "
var per7 = " "
var per8 = " "
var per9 = " "
var per10 = " "
var res = 'res'
if(execution.getVariable('sDocEmploymentHistory') != null){
per4 = "&lt;li&gt;Витяг із трудової книжки.&lt;/li&gt;"
}
if(execution.getVariable('sDocInternship') != null){
per5 = "&lt;li&gt;Довідка про проходження стажування.&lt;/li&gt;"
}
if(execution.getVariable('sDocDiploma') != null){
per6 = "&lt;li&gt;Копія диплома про вищу освіту.&lt;/li&gt;"
}
if(execution.getVariable('sDocForeignDiploma') != null){
per7 = "&lt;li&gt;Копія довідки про визнання іноземного документа про освіту.&lt;/li&gt;"
}
if(execution.getVariable('sDocScientificDegree') != null){
per8 = "&lt;li&gt;Копія документа для підтвердження наукового ступеня.&lt;/li&gt;"
}
if(execution.getVariable('sDocLearning') != null){
per9 = "&lt;li&gt;Копія свідоцтва про проходження навчання з екологічного аудиту.&lt;/li&gt;"
}
if(execution.getVariable('sDocFoto') != null){
per10 = "&lt;li&gt;Фотокартка.&lt;/li&gt;"
}
res = '&lt;ol&gt;'+per1+per2+per3+per4+per5+per6+per7+per8+per9+per10+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsLetter', res)</script>
    </scriptTask>
