        <activiti:formProperty id="fVarDod_07_02" name="Додаток-02 ; ;bNew=true" type="file"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_07_03" name="Додаток-03 ; ;bNew=true" type="file"></activiti:formProperty>
