        <activiti:formProperty id="sUserComment" name="Коментар/питання від заявника" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="sTextMail" name="Якщо за даним зверненням питання до заявника відсутні - вкажіть текст який буде додано до завершуючого листа Заявнику" type="textArea" default="Шановний (-а), " required="true"></activiti:formProperty>
        <activiti:formProperty id="fDecisionDoc" name="Файл-додаток до листа заявнику ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        
        <activiti:formProperty id="fVarDod_01" name="Заява; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_02" name="Протокол; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_03" name="Реєстр; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_04" name="Відомості; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_05" name="Статут; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_06" name="Документ представника; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_07_01" name="Додаток-01 ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_07_02" name="Додаток-02 ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="fVarDod_07_03" name="Додаток-03 ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
   
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <serviceTask id="servicetask4" name="Е-mail повідомлення заявнику про виконану заявку" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[iGov - Ваше звернення на тему: ${sNameService}.]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
Ваше звернення <strong>№[sID_Order]</strong> на тему <strong>"${sNameService}</strong>", опрацьовано.<br /><br />
${sTextMail}<br /><br />
Зверніть увагу на додаток до листа.<br /><br /> 
[pattern/mail/_common_signature_start.html]
[pattern/mail/_common_signature_end.html]
[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[${fDecisionDoc}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="servicetask4"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
