    <scriptTask id="scripttask3" name="Скрипт для печати 2" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1=""
var per2=""
var src=execution.getVariable('lastName')
for (var i = 0; i&lt;50; i++){
var lname=src.substr(i,1)
if((lname != "null") || (lname != "")){
if((lname == "-") || (lname == "`") || (lname == " ") || (lname == "'") || (lname == "’")){
per1 = per1+"&amp;#60"}
else {
per1 = per1+lname}}
else{
break}}
var src=execution.getVariable('firstName')
for (var i = 0; i&lt;50; i++){
var fname=src.substr(i,1)
if((fname != "null") || (fname != "")){
if((fname == "-") || (fname == "`") || (fname == " ") || (fname == "'") || (fname == "’")){
per2 = per2+"&amp;#60"}
else {
per2 = per2+fname}}
else{
break}}
var moz1=per1+"&amp;#60"+"&amp;#60"+per2
execution.setVariable('sMoz1', moz1)
var numb=execution.getVariable('sNumber')
var ser=numb.substr(0,2)
var num=numb.substr(2,6)
var a1=Number(numb.substr(2,1))
var a2=Number(numb.substr(3,1))
var a3=Number(numb.substr(4,1))
var a4=Number(numb.substr(5,1))
var a5=Number(numb.substr(6,1))
var a6=Number(numb.substr(7,1))
var k1=(10*7+11*3+a1*7+a2*3+a3*1+a4*7+a5*3+a6*1)%10
var src=execution.getVariable('sDate_of_birth1')
var day=src.substr(0,2)
var month=src.substr(3,2)
var year=src.substr(6,4)
var month1='СІЧ / JAN'
if (month=='02'){
    month1='ЛЮТ / FEB'}
if (month=='03'){
    month1='БЕР / MAR'}
if (month=='04'){
    month1='КВІ / APR'}
if (month=='05'){
    month1='ТРА / MAY'}
if (month=='06'){
    month1='ЧЕР / JUN'}
if (month=='07'){
    month1='ЛИП / JUL'}
if (month=='08'){
    month1='СЕР / AUG'}
if (month=='09'){
    month1='ВЕР / SEP'}
if (month=='10'){
    month1='ЖОВ / OCT'}
if (month=='11'){
    month1='ЛИС / NOV'}
if (month=='12'){
    month1='ГРУ / DEC'}
var birth=day+' '+month1+' '+year
var year1=src.substr(8,2)
var b1=Number(year1.substr(0,1))
var b2=Number(year1.substr(1,1))
var b3=Number(month.substr(0,1))
var b4=Number(month.substr(1,1))
var b5=Number(day.substr(0,1))
var b6=Number(day.substr(1,1))
var k2=(b1*7+b2*3+b3*1+b4*7+b5*3+b6*1)%10
execution.setVariable('sDate_of_birth', birth)
var sex=execution.getVariable('asSex')
var sex1='Ч / M'
if (sex=='sWoman'){
    sex1='Ж / W'}
execution.setVariable('sSex', sex1)
var unixdate= Math.round((new Date()).getTime());
var new_src = new Date(unixdate);
var d = new_src.getDate();
if (d &lt; 10) d = "0"+d;
var m = new_src.getMonth();
var m = (+m + +1);
if (m &lt; 10) m = "0"+m;
var new_year = new_src.getFullYear();
var month3='СІЧ / JAN'
if (m=='02'){
    month3='ЛЮТ / FEB'}
if (m=='03'){
    month3='БЕР / MAR'}
if (m=='04'){
    month3='КВІ / APR'}
if (m=='05'){
    month3='ТРА / MAY'}
if (m=='06'){
    month3='ЧЕР / JUN'}
if (m=='07'){
    month3='ЛИП / JUL'}
if (m=='08'){
    month3='СЕР / AUG'}
if (m=='09'){
    month3='ВЕР / SEP'}
if (m=='10'){
    month3='ЖОВ / OCT'}
if (m=='11'){
    month3='ЛИС / NOV'}
if (m=='12'){
    month3='ГРУ / DEC'}
var issue=d+' '+month3+' '+new_year
var year3= new_year-(-5)
var expiry=d+' '+month3+' '+year3
execution.setVariable('sDate_of_issue', issue) 
execution.setVariable('sDate_of_expiry', expiry)
var src=execution.getVariable('sSex')
var sex=src.substr(4,1)
var year4=String(year3).substr(2,2)
var c1=Number(year4.substr(2,1))
var c2=Number(year4.substr(3,1))
var c3=Number(m.substr(0,1))
var c4=Number(m.substr(1,1))
var c5=Number(d.substr(0,1))
var c6=Number(d.substr(1,1))
k3=(c1*7+c2*3+c3*1+c4*7+c5*3+c6*1)%10
var moz2=ser+"&lt;"+num+k1+"UKR"+year1+month+day+k2+sex+year4+m+d+k3
execution.setVariable('sMoz2', moz2)
       </script>
