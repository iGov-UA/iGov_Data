    <scriptTask id="scripttask11" name="Формировка даты" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var delta='10';
var unixdate= Math.round((new Date()).getTime());
var unixdate=unixdate+86400*1000*delta;
var new_src = new Date(unixdate);
var d = new_src.getDate();
if (d &lt; 10) d = "0"+d;
var m = new_src.getMonth();
var m = (+m + +1);
if (m &lt; 10) m = "0"+m;

var new_year = new_src.getFullYear();
var new_hour = new_src.getHours();
var new_minutes = new_src.getMinutes();
var timer = new_year+'-'+m+'-'+d+'T'+new_hour+':'+new_minutes+':00';
execution.setVariable('sDate_of_print', timer)
var Pay="надано квитанцію про сплату"
execution.setVariable('sID_Payment', Pay)
</script>
    </scriptTask>
