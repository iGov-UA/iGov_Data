    <scriptTask id="scripttask1" name="Скрипт для печати 1" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var k1=' '
var k2=' '
var k3=' '
var k4=' '      
var k5=' '
var k6=' '
var k7=' '
var k8=' '      
var k9=' '      
var ship1=execution.getVariable('asUkTaxy')
if (ship1=='sUkTaxyOk'){
k1='внутрішні перевезення пасажирів на таксі, '}
var ship2=execution.getVariable('asWorldTaxy')
if (ship2=='sWorldTaxyOk'){
k2='міжнародні перевезення пасажирів на таксі, '}
var ship3=execution.getVariable('asUkCar')
if (ship3=='sUkCarOk'){
k3='внутрішні перевезення пасажирів легковими автомобілями на замовлення, '}
var ship4=execution.getVariable('asWorldCar')
if (ship4=='sWorldCarOk'){
k4='міжнародні перевезення пасажирів легковими автомобілями на замовлення, '}
var ship5=execution.getVariable('asUkBus')
if (ship5=='sUkBusOk'){
k5='внутрішні перевезення пасажирів автобусами, '}
var ship6=execution.getVariable('asWorldBus')
if (ship6=='sWorldBusOk'){
k6='міжнародні перевезення пасажирів автобусами, '}
var ship7=execution.getVariable('asUkTrash')
if (ship7=='sUkTrashOk'){
k7='внутрішні перевезення небезпечних вантажів та небезпечних відходів вантажними автомобілями, '}
var ship8=execution.getVariable('asWorldTrash')
if (ship8=='sWorldTrashOk'){
k8='міжнародні перевезення небезпечних вантажів та небезпечних відходів вантажними автомобілями, '}
var ship9=execution.getVariable('asWorldTrack')
if (ship9=='sWorldTrackOk'){
k9='міжнародні перевезення вантажів вантажними автомобілями (крім перевезення небезпечних вантажів та небезпечних відходів)'}
var res=k1+k2+k3+k4+k5+k6+k7+k8+k9     
execution.setVariable('sTypeBiz', res)
	  </script>
