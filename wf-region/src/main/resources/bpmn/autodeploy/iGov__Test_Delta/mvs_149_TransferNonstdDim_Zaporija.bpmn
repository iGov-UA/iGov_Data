    <scriptTask id="scripttask1" name="Скріпт файл" scriptFormat="javascript" activiti:autoStoreVariables="false">
<script>
var file1 = '&lt;li&gt;Заява.&lt;/li&gt;'
var file2 = ' '
var file3 = ' '
var file4 = ' '
var file5 = ' '
var file6 = ' '
var file7 = ' '
var file8 = ' '
var res = 'res'
if(execution.getVariable('oplata') != null){
  file2 = '&lt;li&gt; Квитанція про оплату вартості послуги.&lt;/li&gt;'
}
if(execution.getVariable('bankId_scan_passport') != null){
  file3 = '&lt;li&gt; Паспорт заявника.&lt;/li&gt;'
}
if(execution.getVariable('nFile_DocReject') != null){
  file4 = '&lt;li&gt; Відмова від адміністративного органу при первинному поданні документів.&lt;/li&gt;'
}
if(execution.getVariable('nFile_ConfidantDoc') != null){
  file5 = '&lt;li&gt; Документ, що засвідчує повноваження довіреної особи.&lt;/li&gt;'
}
if(execution.getVariable('sDocCommunal') != null){
  file6 = '&lt;li&gt; Визначений та погоджений з власниками (комунальними і дорожніми організаціями) вулично-дорожніх мереж маршруту руху.&lt;/li&gt;'
}
if(execution.getVariable('sDocRailway') != null){
  file7 = '&lt;li&gt; Погодження власників залізничних переїздів, мостового господарства, та за необхідності, служб міського електротранспорту, електромереж, електрифікації, електрозв’язку.&lt;/li&gt;'
}
if(execution.getVariable('sDocBuild') != null){
  file8 = '&lt;li&gt; Висновок за результатами проведення спеціалізованого обстеження або випробування будівель, споруд і мереж на маршруті.&lt;/li&gt;'
}

res = '&lt;ol&gt;'+file1+file2+file3+file4+file5+file6+file7+file8+'&lt;/ol&gt;'

execution.setVariable('tDokumentsList', res)
</script>  
