    <scriptTask id="scripttask1" name="Script1" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var yyyy = ''
var mmm = ''
var ddd = ''
var timeB = ''
var yyyy2 = ''
var mmm2 = ''
var ddd2 = ''
var timeE = ''
var eest = ''
    timeB=execution.getVariable('date_bigin').toString()
    eest=timeB.substr(20,4)
    if(eest == 'EEST'){yyyy=timeB.substr(25,4)}
    else{yyyy=timeB.substr(24,4)}
    mmm=timeB.substr(4,3)
if (mmm == 'Jan'){ mmm='01'}
if (mmm == 'Feb'){ mmm='02'}
if (mmm == 'Mar'){ mmm='03'}
if (mmm == 'Apr'){ mmm='04'}
if (mmm == 'May'){ mmm='05'}
if (mmm == 'Jun'){ mmm='06'}
if (mmm == 'Jul'){ mmm='07'}
if (mmm == 'Aug'){ mmm='08'}
if (mmm == 'Sep'){ mmm='09'}
if (mmm == 'Oct'){ mmm='10'}
if (mmm == 'Nov'){ mmm='11'}
if (mmm == 'Dec'){ mmm='12'}
	ddd=timeB.substr(8,2)
timeB = ddd+'-'+mmm+'-'+yyyy
yyyy2 = yyyy-(-1)
if (ddd == '01')
  {
  mmm2=mmm-1
  if (mmm2&lt;10)
    {mmm2 = '0'+mmm2}

  if(mmm =='06'||'09'||'11'||'04'||'02'||'08')
    {ddd2='31'}
  else
    {ddd2='30'}      
      
  if(mmm =='03')
    {ddd2='28'}
  }
else
  {
  mmm2=mmm
  ddd2=ddd-1
  }
if(mmm =='01'&amp;&amp;ddd=='01')
  {
  ddd2='31'
  mmm2='12'
  yyyy2=yyyy
  }
timeE = ddd2+'-'+mmm2+'-'+yyyy2	
execution.setVariable('date_big', timeB)
execution.setVariable('date_end', timeE)
      </script>
    </scriptTask>
