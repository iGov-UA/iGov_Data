    <sequenceFlow id="flow34" name="ВІДМОВА" sourceRef="exclusivegateway2" targetRef="servicetask8">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asDicide1 == 'sEnamReject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow66" name="ЕЛЕКТРОННА ВІДПОВІДЬ" sourceRef="exclusivegateway2" targetRef="servicetask15">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asDicide1 == 'sEnamAccept' && asVidpovid == 'sEnamECP'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow64" name="БЕЗ ПОШТИ" sourceRef="exclusivegateway2" targetRef="servicetask14">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asDicide1 == 'sEnamAccept' && asVidpovid == 'sEnamnarochno'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow65" name="ЛИСТ ОПЛАТИ" sourceRef="exclusivegateway2" targetRef="scripttask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asDicide1 == 'sEnamAccept' && asVidpovid == 'sEnamPost'}]]></conditionExpression>
    </sequenceFlow>
