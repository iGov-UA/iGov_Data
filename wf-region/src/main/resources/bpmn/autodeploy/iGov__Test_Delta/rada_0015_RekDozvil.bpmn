    <scriptTask id="scripttask2" name="Script Task (JavaScript2)" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
	var src=execution.getVariable('date_today');
	execution.setVariable('sDateRegistration', src);
	
	var per1 = ' '
	var per2 = 0
	var sStartTag =''		 
	var sEndTag =''		 

	if (execution.getVariable('nFile_PlaceFoto') != null &amp;&amp; execution.getVariable('nFile_PlaceFoto')!= ' ')
	{
		per2 = per2 + 1
		per1 = per1 + sStartTag + per2.toString() + '. Фото місця розташування. &lt;br&gt;' + sEndTag 
	}
	if (execution.getVariable('nFile_Eskiz') != null  &amp;&amp; execution.getVariable('nFile_Eskiz')!= ' ')
	{
		per2 = per2 + 1
		per1 = per1 + sStartTag + per2.toString() + '. Еcкіз рекламної конструкції. &lt;br&gt;' + sEndTag  
	}
	if (execution.getVariable('nFile_PravoVlasnist') != null  &amp;&amp; execution.getVariable('nFile_PravoVlasnist')!= ' ')
	{
		per2 = per2 + 1
		per1 = per1 + sStartTag + per2.toString() + '. Правовстановлюючи документи на місце розташування рекламного засобу. &lt;br&gt;' + sEndTag  
	}
	if (execution.getVariable('nFile_Dovirenist') != null  &amp;&amp; execution.getVariable('nFile_Dovirenist')!= ' ')
	{
		per2 = per2 + 1
		per1 = per1 + sStartTag + per2.toString() + '. Копія довіреності на представника. &lt;br&gt;' + sEndTag  
	}

	execution.setVariable('sAddDoc', per1) 
	
	  </script>
    </scriptTask>
