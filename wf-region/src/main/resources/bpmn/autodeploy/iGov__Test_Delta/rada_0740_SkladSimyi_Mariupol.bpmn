    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var pasZ1 = execution.getVariable('bankIdlastName')
var pasZ2 = execution.getVariable('bankIdfirstName')
var pasZ3 = execution.getVariable('bankIdmiddleName')
var pasZ4 = execution.getVariable('bankIdinn')
var pasZ5 = execution.getVariable('bankIdPassport')
var pasD1 = execution.getVariable('DovirutLastName')
var pasD2 = execution.getVariable('DovirutFirstName')
var pasD3 = execution.getVariable('DovirutMiddleName')
var pasD4 = execution.getVariable('DovirutdInn')
var pasD5 = execution.getVariable('DovirutdPassport')

if(execution.getVariable('asStatus') == 'sVlasn') execution.setVariable('pasZ1', ScriptLastName);
  {
  execution.setVariable('ScriptLastName', pasZ1)
  execution.setVariable('ScriptFirstName', pasZ2)
  execution.setVariable('ScriptMiddleName', pasZ3)
  execution.setVariable('ScriptInn', pasZ4)
  execution.setVariable('ScriptPassport', pasZ5)
  }
if(execution.getVariable('asStatus') == 'sZarey')
  {
  execution.setVariable('ScriptLastName', pasZ1)
  execution.setVariable('ScriptFirstName', pasZ2)
  execution.setVariable('ScriptMiddleName', pasZ3)
  execution.setVariable('ScriptInn', pasZ4)
  execution.setVariable('ScriptPassport', pasZ5)
  }
if(execution.getVariable('asStatus') == 'sVlasnDov')
  {
  execution.setVariable('ScriptLastName', pasD1)
  execution.setVariable('ScriptFirstName', pasD2)
  execution.setVariable('ScriptMiddleName', pasD3)
  execution.setVariable('ScriptInn', pasD4)
  execution.setVariable('ScriptPassport', pasD5)
  }
if(execution.getVariable('asStatus') == 'sZareyDov')
  {
  execution.setVariable('ScriptLastName', pasD1)
  execution.setVariable('ScriptFirstName', pasD2)
  execution.setVariable('ScriptMiddleName', pasD3)
  execution.setVariable('ScriptInn', pasD4)
  execution.setVariable('ScriptPassport', pasD5)
  }

var TypeVul = ' '
if(execution.getVariable('asTypeVul') == 'sEnamVul'){TypeVul='вулиця'}
if(execution.getVariable('asTypeVul') == 'sEnamProv'){TypeVul='провулок'}
if(execution.getVariable('asTypeVul') == 'sEnamProsp'){TypeVul='проспект'}
if(execution.getVariable('asTypeVul') == 'sEnamBulv'){TypeVul='бульвар'}
if(execution.getVariable('asTypeVul') == 'sEnamNaber'){TypeVul='набережна'}
if(execution.getVariable('asTypeVul') == 'sEnamUzviz'){TypeVul='узвіз'}
if(execution.getVariable('asTypeVul') == 'sEnamTupik'){TypeVul='тупик'}
if(execution.getVariable('asTypeVul') == 'sEnamStanc'){TypeVul='станція'}
if(execution.getVariable('asTypeVul') == 'sEnamSose'){TypeVul='шосе'}
if(execution.getVariable('asTypeVul') == 'sEnamPlosh'){TypeVul='площа'}
if(execution.getVariable('asTypeVul') == 'sEnamGM'){TypeVul='жилий масив'}
if(execution.getVariable('asTypeVul') == 'sEnamProy'){TypeVul='проїзд'}
if(execution.getVariable('asTypeVul') == 'sEnamSpusk'){TypeVul='спуск'}

var Vul = execution.getVariable('sRegistrationVul')
var Bud = execution.getVariable('sRegistrationBud')
var Korp = execution.getVariable('lRegistrationKorp')
var Kvart = execution.getVariable('lRegistrationKvart')
var Adres = ' ' 
  
if(execution.getVariable('asBudType') == 'sBagkvatur')
  {
  if(execution.getVariable('asKorp') == 'yes'){Adres='м.Маріуполь, '+TypeVul+' '+Vul+', б. '+Bud+', корп. '+Korp+', кв. '+Kvart}
  if(execution.getVariable('asKorp') == 'no'){Adres='м.Маріуполь, '+TypeVul+' '+Vul+', б. '+Bud+', кв. '+Kvart}
  }
if(execution.getVariable('asBudType') == 'sPrivSector')
  {
  if(execution.getVariable('asKvart') == 'yes'){Adres='м.Маріуполь, '+TypeVul+' '+Vul+', б. '+Bud+', кв. '+Kvart}
  if(execution.getVariable('asKvart') == 'no'){Adres='м.Маріуполь, '+TypeVul+' '+Vul+', б. '+Bud}
  }  
execution.setVariable('ScriptAdress', Adres)

      </script>
