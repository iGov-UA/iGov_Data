    <scriptTask id="scripttask1" name="Script  pulling data" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
		var per4 = 'substance'
		var per5 = 'substance_2'
		var per6 = 'substance_3'
		var per7 ='term'
		if (execution.getVariable('vibor') == 'prav')
		{
		per1 = '23'
		per2 = 'woobraja@mail.ru'
		per3 = 'Головному державному санітарному лікарю Дніпропетровської області'
		}
		if (execution.getVariable('vibor') == 'lev')
		{
		per1 = '24'
		per2 = 'Luda17_@ukr.net'
		per3 = 'Головному державному санітарному лікарю Дніпропетровської області'
		}
		if (execution.getVariable('vibor_service') == 'issue')
		{
		per4 = 'видати'
		per5 = 'Ваш висновок державної санітарно-епідеміологічної експертизи матеріалів, у яких обґрунтовується зменшення розмірів санітарно-захисної зони об’єкту IV, V класів небезпеки - готовий'
		per6 = 'видачі'
		per7 = 'тридцяти календарних днів'
		}
		if (execution.getVariable('vibor_service') == 'restructuring')
		{
		per4 = 'переоформити'
		per5 = 'Ваш переоформлений висновок державної санітарно-епідеміологічної експертизи матеріалів, у яких обґрунтовується зменшення розмірів санітарно-захисної зони об’єкту IV, V класів небезпеки - готовий'
		per6 = 'переоформлення'
		per7 = 'двох робочих днів'
		}
		if (execution.getVariable('vibor_service') == 'duplicate')
		{
		per4 = 'видати дублікат'
		per5 = 'Ваш дублікат висновку державної санітарно-епідеміологічної експертизи матеріалів, у яких обґрунтовується зменшення розмірів санітарно-захисної зони об’єкту IV, V класів небезпеки - готовий'
		per6 = 'видачі дублікату'
		per7 = 'двох робочих днів'
		}
		if (execution.getVariable('vibor_service') == 'cansel')
		{
		per4 = 'анулювати'
		per5 = 'Рішення про анулювання документа дозвільного характеру - готове'
		per6 = 'анулювання'
		per7 = 'п`яти робочих днів'
		}
		execution.setVariable('substance', per4)
		execution.setVariable('substance_2', per5)
		execution.setVariable('substance_3', per6)
		execution.setVariable('term', per7)</script>
    </scriptTask>
    <scriptTask id="scripttask2" name="Script list_documents" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
      	var per1 = ' '
		var per2 = ' '
		var per3 = ' '
		var per4 = ' '
		var per5 = ' '
		var per6 = ' '
		var res = 'res'
		if (execution.getVariable('vibor_service') == 'issue') 
		{
		per1 = '&lt;li&gt;Документ, що підтверджує внесення плати за комплексне вивчення документів та підготовку проекту висновку державної санітарно-епідеміологічної експертизи.&lt;/li&gt;'
		per2 = "&lt;li&gt;Пректні матеріали, у яких обгрунтовується зменшення розмірів санітарно-захисної зони об'єктів IV, V класів небезпеки.&lt;/li&gt;"
		per3 = "&lt;li&gt;Акт обстеження (інспектування) об'єкта отримання документу дозвільного характеру для потреб державної санітарно-епідеміологічної експертизи.&lt;/li&gt;"
		per4 = "&lt;li&gt;Копії звіту (звітів) про результати робіт для потреб державної санітарно-епідеміологічної експертизи.&lt;/li&gt;"
		res = '&lt;ol&gt;'+per1+per2+per3+per4+'&lt;/ol&gt;'
		}
		if (execution.getVariable('vibor_service') == 'restructuring') 
		{
		per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає переоформленню.&lt;/li&gt;'
		per2 = '&lt;li&gt;Документ, який засвідчує настання підстави для переоформлення документу дозвільного характеру.&lt;/li&gt;'
		res = '&lt;ol&gt;'+per1+per2+'&lt;/ol&gt;'
		}
		if (execution.getVariable('vibor_service') == 'duplicate') 
		{
		per1 = '&lt;li&gt;Бланк документу дозвільного характеру, який непридатний для використання.&lt;/li&gt;'
		res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
		}
		if (execution.getVariable('vibor_service') == 'cansel') 
		{
		per1 = '&lt;li&gt;Документ дозвільного характеру, який підлягає анулюванню.&lt;/li&gt;'
		res = '&lt;ol&gt;'+per1+'&lt;/ol&gt;'
		}
		execution.setVariable('list_documents', res)</script>
    </scriptTask>
