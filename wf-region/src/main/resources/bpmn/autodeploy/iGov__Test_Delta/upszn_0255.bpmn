        <activiti:formProperty id="email" name="Адреса електронної пошти для зв'язку" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Тип звернення" type="enum" required="true">
          <activiti:value id="sEnumFirst" name="Надання статусу та видача посвідчення «Жертва нацистських переслідувань»"></activiti:value>
          <activiti:value id="sEnumChange" name="Заміна посвідчення «Жертва нацистських переслідувань»"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sPrich" name="Причина заміни посвідчення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="markersLine_1" name="Документи" type="markers" default="{&quot;attributes&quot;:{&quot;Line_1&quot;:{&quot;aElement_ID&quot;:[&quot;ScanPassport&quot;],&quot;sValue&quot;:&quot;Документи&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="ScanPassport" name="Скан-копія паспорта: сторінки 1-3 та відомості про реєстрацію місця проживання; декілька сторінок об'єднайте у один .pdf файл або .zip архів ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_inn" name="Скан-копія картки платника податків (довідки про присвоєння індивідуального податкового номеру ІПН) ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nNazi" name="Довідка про факт нацистських переслідувань ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nFamily" name="Довідка про склад сім'ї ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="sSubstance" name="&quot;Жертва нацистських переслідувань&quot; ; ;bVisible=false" type="string" default="&quot;Жертва нацистських переслідувань&quot;"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="Телефон для довідок ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="Назва органа ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Час роботи ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта держслужбовця ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="nArea" name="Графік роботи ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="markers1" name="Вибір категорії" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;sPrich&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asType&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'sEnumChange'&quot;}}}"></activiti:formProperty>
