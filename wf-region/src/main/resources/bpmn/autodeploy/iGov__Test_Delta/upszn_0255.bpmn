    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="servicetask2" name="Повідомлення держслужбовця про звернення" activiti:async="true" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${sMailClerk}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення. Надання статусу та видача (заміна) посвідчення «Жертва нацистських переслідувань»]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Направлення громадянину листа про отримання звернення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[Vashe zvernennya [sID_Order] zareestrovano]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.gov.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему "Надання статусу та видача (заміна) посвідчення «Жертва нацистських переслідувань» прийняте]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
[pattern/mail/new_design/_common_content_start.html]
<p> Ваше звернення №[sID_Order] на тему: enum{[asType]} було зареєстровано у системі.<br>
Повідомлення про результат послуги буде спрямоване на Ваш e-mail.</p>
<br>
${sPhoneOrgan}<br>
[pattern/mail/new_design/_common_content_end.html]
[pattern/mail/new_design/_common_signature_start.html]
${sNameOrgan}<br />
[pattern/mail/new_design/_common_signature_end.html]
[pattern/mail/new_design/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <userTask id="usertask1" name="Розгляд документів держслужбовцем" activiti:candidateGroups="upszn${nArea}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Суб’єкт звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdAddressFactual" name="Адреса реєстрації громадянина; ;writable=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="bankIdinn" name="ІПН" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний номер телефону" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти для зв'язку" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Тип звернення" type="enum" writable="false">
          <activiti:value id="sEnumFirst" name="Надання статусу та видача посвідчення «Жертва нацистських переслідувань»"></activiti:value>
          <activiti:value id="sEnumChange" name="Заміна посвідчення «Жертва нацистських переслідувань»"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sPrich" name="Причина заміни посвідчення" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asEnumStatus" name="Обробка заяви" type="enum" required="true">
          <activiti:value id="sEnumApprove" name="Задовільнити заяву"></activiti:value>
          <activiti:value id="sEnumDeny" name="Відмова громадянину у разі надання неповного пакету документів"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="decisionComment" name="Якщо ви відмовляєте замовнику у наданні послуги, обов'язково вкажіть підставу у цьому полі." type="textArea"></activiti:formProperty>
        <activiti:formProperty id="sSubstance" name="«Жертва нацистських переслідувань» ; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="PrintForm_1" name="File label;File title;pattern/print/UPSZN/kalush_soczah_401_print.html" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sBody1" name="[pattern/print/UPSZN/kalush_soczah_401_print.html] ; ;bVisible=false ,bPrintform=true" type="string" default="Заява громадянина" writable="false"></activiti:formProperty>
        
        <activiti:formProperty id="ScanPassport" name="Скан-копія паспорта: сторінки 1-3 та відомості про реєстрацію місця проживання; декілька сторінок об'єднайте у один .pdf файл або .zip архів ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_inn" name="Скан-копія картки платника податків (довідки про присвоєння індивідуального податкового номеру ІПН) ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nNazi" name="Довідка про факт нацистських переслідувань ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="nFamily" name="Довідка про склад сім'ї ; ;bNew=true" type="file" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        
      </extensionElements>
