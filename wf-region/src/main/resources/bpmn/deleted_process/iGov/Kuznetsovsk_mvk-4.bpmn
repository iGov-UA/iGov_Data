    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask2"></sequenceFlow>
    <serviceTask id="servicetask2" name="Підтягування документів" activiti:delegateExpression="#{procedQueueTickets}"></serviceTask>
