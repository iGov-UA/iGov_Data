        <activiti:formProperty id="instrukcii" name="Зазначте наявність інструкцій з охорони праці;перелік інструкцій декларуємих робіт, машин та механізмів" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="instruktag" name="Надайте інформацію про проведення навчання та інструктажу з питань охорони праці;протокол, номер, дата та навчальний заклад" type="textArea" required="true"></activiti:formProperty>
