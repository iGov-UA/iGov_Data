          <activiti:value id="accept" name="схвалено призначення допомоги"></activiti:value>
          <activiti:value id="reject" name="відмовлено у призначенні допомоги"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="comment_1" name="У разі відмови внесіть, будь ласка, коментар" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="sBody_1" name="[pattern/print/dnepr_soc_help_148_print.html]" type="invisible" default="заява громадянина" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression>${bankId_scan_passport}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
