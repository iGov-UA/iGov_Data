    <userTask id="usertask2_red" name="Візит громадянина" activiti:candidateGroups="forma3_${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Назва органу" writable="false"></activiti:formProperty>
        <activiti:formProperty id="markers2" name="VisitDay" type="markers" default="${markerService.loadFromFile('form3/form3_Subject.json')}"></activiti:formProperty>
        
        <activiti:formProperty id="info" name="На цьому етапі:" type="label" default="Ви перевіряєте дані громадянина і вирішуєте чи всі вони вірні. Якщо все добре, то громадянин має прийти до Вас у вказаний день і час для отримання довідки про склад сім'ї"></activiti:formProperty>
        <activiti:formProperty id="sCancelInfo" name="Чи скасована заявка (заповнюється громадянином лише у разі відміни запису, за замовчуванням вказано &quot;Заявка актуальна&quot;)" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="visitDay" name="День та час зустрічі: " type="queueData" writable="false"></activiti:formProperty>
        <activiti:formProperty id="VizitData" name="День коли ви хочете прийти за довідкою; ;writable=false" type="date"></activiti:formProperty>
        <activiti:formProperty id="sKud" name="Для чого потрібка довідка?; ;writable=false" type="string"></activiti:formProperty>
        
        
        <activiti:formProperty id="markersLine_11" name="Інформація про заявника" type="markers" default="{&quot;attributes&quot;:{&quot;Line_11&quot;:{&quot;aElement_ID&quot;:[&quot;clFio&quot;],&quot;sValue&quot;:&quot;Інформація про заявника&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="clFio" name="ПІБ заявника; ;writable=false" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="DateBirth" name="Дата народження; ;writable=false" type="date"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса для зворотнього зв'язку" writable="false"></activiti:formProperty>
        <activiti:formProperty id="RegistrationAddress" name="Адреса реєстраці; ;writable=false" type="string"></activiti:formProperty>
        
        <activiti:formProperty id="markersLine_22" name="Інформація про інших осіб що проживають з заявником" type="markers" default="{&quot;attributes&quot;:{&quot;Line_22&quot;:{&quot;aElement_ID&quot;:[&quot;asPeople&quot;],&quot;sValue&quot;:&quot;Інформація про інших осіб що проживають з заявником&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="asPeopleChek" name="Чи зареєстровані разом з заявником інші особи?;позначте галочкою - якщо так;sID_CheckboxTrue=yes" type="enum"  writable="false">
          <activiti:value id="yes" name="так"></activiti:value>
          <activiti:value id="no" name="ні"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="asPeople" name="Яка кількість осіб зареєстрована за цією адресою?" type="enum"  writable="false">
          <activiti:value id="one" name="1"></activiti:value>
          <activiti:value id="two" name="2"></activiti:value>
          <activiti:value id="three" name="3"></activiti:value>
          <activiti:value id="four" name="4"></activiti:value>
          <activiti:value id="five" name="5"></activiti:value>
          <activiti:value id="six" name="6"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="PeopleTable" name="Дані про інших осіб; ;bNew=true" type="table"  writable="false"></activiti:formProperty>
        
        <activiti:formProperty id="result" name="Результат зустрічі" type="enum" required="true">
          <activiti:value id="reestr" name="Громадянин отримав довідку"></activiti:value>
          <activiti:value id="nejav" name="Громадянин не з'явився"></activiti:value>
        </activiti:formProperty>
        
        <activiti:formProperty id="markers3" name="Настроювання принтформи " type="markers" default="${markerService.loadFromFile('dms/forma_3.json')}"></activiti:formProperty>
