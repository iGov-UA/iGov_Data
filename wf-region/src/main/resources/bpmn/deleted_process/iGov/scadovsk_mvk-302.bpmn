        <activiti:formProperty id="sBody_2" name="[pattern/print/dms/forma3_1.html]" type="invisible" default="Довідка про склад сім'ї (розширена)" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sBody_3" name="[pattern/print/dms/forma3_lubin.html]" type="invisible" default="Довідка про склад сім'ї (Великий Любінь)" writable="false"></activiti:formProperty>
