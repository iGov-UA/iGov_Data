    <scriptTask id="scripttask1" name="Edit Email" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per4 = 'substance'
var per5 = 'substance_2'
var per6 = 'substance_3'
var per7 ='term'
if (execution.getVariable('vibor_service') == 'issue'){
per4 = 'видати'
per5 = 'Ваш дозвіл на всі види досліджень та експериментальні роботи з мікроорганізмами ІІІ-IV групи патогенності (небезпеки) або матеріалом підозрілим на їх вміст - готовий'
per6 = 'видачі'
per7 = 'десяти робочих днів'
}
if (execution.getVariable('vibor_service') == 'restructuring'){
per4 = 'переоформити'
per5 = 'Ваш переоформлений дозвіл на всі види досліджень та експериментальні роботи з мікроорганізмами ІІІ-IV групи патогенності (небезпеки) або матеріалом підозрілим на їх вміст - готовий'
per6 = 'переоформлення'
per7 = 'двох робочих днів'
}
if (execution.getVariable('vibor_service') == 'duplicate'){
per4 = 'видати дублікат'
per5 = 'Ваш дублікат дозволу на всі види досліджень та експериментальні роботи з мікроорганізмами ІІІ-IV групи патогенності (небезпеки) або матеріалом підозрілим на їх вміст - готовий'
per6 = 'видачі дублікату'
per7 = 'двох робочих днів'
}
if (execution.getVariable('vibor_service') == 'cansel'){
per4 = 'анулювати'
per5 = 'Рішення про анулювання документа дозвільного характеру - готове'
per6 = 'анулювання'
per7 = 'п`яти робочих днів'
}
execution.setVariable('substance', per4)
execution.setVariable('substance_2', per5)
execution.setVariable('substance_3', per6)
execution.setVariable('term', per7)</script>
