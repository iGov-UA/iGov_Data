        <activiti:formProperty id="rNumber" name="номер району" type="invisible" default="635" writable="false"></activiti:formProperty>
        <activiti:formProperty id="topic" name="Необхідно отримати згоду на" type="enum" writable="false">
         <activiti:value id="Enum1" name="будівництво об'єкта сервісу"></activiti:value>
          <activiti:value id="Enum2" name="будівництво об'єкта автозаправної станції"></activiti:value>
          <activiti:value id="Enum3" name="будівництво об'єкта прокладення інженерних мереж"></activiti:value>
          <activiti:value id="Enum4" name="виконання інших робіт у межах смуги відведення  автомобільних доріг"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="visitDay" name="День та час зустрічі" type="queueData " writable="false"></activiti:formProperty>
        <activiti:formProperty id="squer_obj" name="Площа об'єкта; м.кв." type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Adress_obj" name="Місце знаходження об'єкта" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="other" name="Отримання дозволу на" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file1" name="Погодження з власником вулично-дорожньої мережі" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file2" name="Схема &quot;Організація дорожнього руху&quot; робочого проекту на розміщення об'єкту" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file3" name="Топографічний знімок місцевості (М 1:500) з прив'язкою місця розташування об'єкту" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file4" name="Паспорт з прив'язкою об'єкту" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file5" name="Акт відведення земельної ділянки" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file6" name="Рішення власника дороги" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="file7" name="Технічні умови власника дороги" type="file" writable="false"></activiti:formProperty>
        <activiti:formProperty id="result" name="Рішення держслужбовця" type="enum" required="true">
          <activiti:value id="da" name="Призначити зустріч у відділенні"></activiti:value>
          <activiti:value id="net" name="Відмовити у наданні дозволу"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="fileresult" name="Додайте,будь ласка, файл з рішенням" type="file" required="true"></activiti:formProperty>
