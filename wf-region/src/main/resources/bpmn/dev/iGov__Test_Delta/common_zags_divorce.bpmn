      <script>  execution.setVariable('sDateAppeal', new Date().format("dd.MM.yyyy").toString())
      def sum =' ' 
      execution.setVariable('sSum', sum);
       if(execution.getVariable('sWithoutCitizenship1') == 'sEnum1')  { 
        execution.setVariable('sCitizenship1', ' ')};
        if(execution.getVariable('sWithoutCitizenship2') == 'sEnum1')  { 
        execution.setVariable('sCitizenship2', ' ')};
        execution.setVariable('sDescription', 'державне мито за послугу розірвання шлюбу'); 
        
      </script>
    </scriptTask>
